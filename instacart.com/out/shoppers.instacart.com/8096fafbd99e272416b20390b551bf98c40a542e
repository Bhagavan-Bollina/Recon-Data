<script src="https://d2nkqaulr5muje.cloudfront.net/assets/applicants/landing_page_v2-1d217d7a66767c60264aa999f92d4696d55be480fd65e0948bf5dd98e06846de.js"></script>
<script>
  (function() {
    (function() {
      var md;
      md = new MobileDetect(window.navigator.userAgent);
      $('#applicant_phone_brand').val(md.os());
      if (md.os() === "iOS") {
        $('#applicant_phone_type').val(md.version('Version'));
      }
      if (md.os() === "AndroidOS") {
        return $('#applicant_phone_type').val(md.version('Android'));
      }
    });
  
  }).call(this);
</script>
