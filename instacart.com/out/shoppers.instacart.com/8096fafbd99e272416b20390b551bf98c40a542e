<script>
  const sendLinkClicked = function() {
    document.getElementById('sent-text').style.display='block';
    let phone = document.getElementById('phone').value.replace(/[^0-9]/g, '');
    analytics_track('shopper-app-download.send_link_via_sms', {'page_type': 'homepage', 'phone': phone});
  };
  
  const storeClicked = function(store, appNativeDownload) {
    analytics_track('shopper-app-download.store', {'page_type': 'homepage', 'app_native_download': appNativeDownload, 'store': store});
  };
  
  const covidClick = function() {
    analytics_track('shopper-app-covid-banner.button', {'page_type': 'homepage'})
  }
</script>

