<script>
  const inputBlurEvent = function(sourceType, sourceValue) {
    analytics_track('applicants.input_focus', {'source_type': sourceType, 'source_value': sourceValue});
  };
  
  const form = document.getElementById("new_applicant");
  const inputs = form.getElementsByTagName("input")
  const validateInputs = function() {
    let valid = true;
    for (let i = 0; i < inputs.length; i++) {
      const currentInput = inputs[i];
      if(currentInput.validity && !currentInput.validity.valid) {
        valid = false;
        analytics_track('applicants.form_error_client', {
          'source_type': currentInput.id,
          'source_value': currentInput.value,
          'validation': currentInput.validity
        })
      }
    }
    return valid;
  }
  
  const submitClick = function(zoneName=null, zoneState=null) {
    zoneData = {}
    if (zoneState) {
      zoneData = {'zone_name': zoneName, 'zone_state': zoneState}
    }
    analytics_track('applicants.form_submit', {'source_type': 'signup_form_submit', ...zoneData});
    validateInputs();
  }
  
  document.addEventListener("DOMContentLoaded", function(){
    var retry = 10;
    var missing_device_id = 'KOCHAVA_DEVICE_ID_MISSING';
  
    function setKochavaId(retry) {
      retry = retry - 1;
  
      if (retry <= 0) {
        document.getElementById('kochava_device_id').value = missing_device_id
        return;
      }
  
      if (kochava && kochava.kvID && kochava.kvID !== "") {
        document.getElementById('kochava_device_id').value = kochava.kvID
        return;
      }
  
      return setTimeout(function() {
        setKochavaId(retry);
      }, 250)
    }
  
    setKochavaId(retry);
  });
</script>

