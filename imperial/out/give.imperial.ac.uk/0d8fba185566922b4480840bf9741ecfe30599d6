       var countdown_event_date = '2017/03/17 0:00:00'

       var now = new Date();
       var eventDate = new Date(countdown_event_date);
      
        if (now > eventDate)
        {
           countdown_event_date = '2017/03/18 0:00:00'
        }
       
        $(document).ready(function () {
            //set width of wrapper;
            $('#no-reflection').county({ endDateTime: new Date(countdown_event_date), reflection: false, animation: 'scroll', theme: 'black' });

        });
    </script>

 
