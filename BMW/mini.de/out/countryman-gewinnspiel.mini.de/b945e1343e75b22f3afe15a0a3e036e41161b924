		<script>
            function checkCookiePermission(optionIdentifier) {
              var name = "pnt-cookie-banner=";
              var ca = document.cookie.split(';');
              for(var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                  c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                  var cookieObject = JSON.parse(c.substring(name.length, c.length));
                  for(var key in cookieObject["perms"]) {
                    for(var id in cookieObject["perms"][key]) {
                        if(id === optionIdentifier) {
                            return cookieObject["perms"][key][id];
                        }
                    }
                  }
                }
              }
              return false;
            }
            </script>
