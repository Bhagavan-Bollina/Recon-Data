		<script type="text/javascript">
			function eceCookieReducer(acc, cookie) {
				var c = cookie.trim().split('=');
				acc[c[0]] = c[1];
				return acc;
			}

			var cookieName = "Cookie";
			var cookies = document.cookie.split(';').reduce(eceCookieReducer, {});

			if(cookies && cookies[cookieName]) {
				document.documentElement.classList.add('app-view');
			} else {
				document.documentElement.classList.add('web-view');
			}

			if (cookies && cookies[cookieName] == "1") {
				document.documentElement.classList.add('app-view--v1');
			}
			if (cookies && cookies[cookieName] == "2") {
				document.documentElement.classList.add('app-view--v2');
			}
