		function hideShow(buttonId) {
			//1-deactivate; 2-activate 
			if(buttonId==="deactivate"){
				
				document.getElementById('headerButtonSetTwo').style.display = "block";
				document.getElementById('headerButtonSetOne').style.display = "none";
				//cookieMngmnt(1);
			}else{
				document.getElementById('headerButtonSetOne').style.display = "block";
				document.getElementById('headerButtonSetTwo').style.display = "none";
				//cookieMngmnt(2);
	    	}	
		}
		
		//turns cookie usage on/off
		function cookieMngmnt(onOff){
			if(onOff===1){  //deactivate
				var ca = document.cookie.split(';');	
				var cookies = {};
				
				if(ca!==null){ 
					for (var i=0; i < ca.length; i++){
						var pair = ca[i].split("=");
						cookies[(pair[0]+'').trim()] = unescape(pair[1]);
						
						for(var j=0; j < cookieDeletionList.length; j++){							
							if(pair[0]===cookieDeletionList[j]){
								document.cookie = pair[0]+"=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;";
							}
						}	
					}
				}
			}else{ //activate
					//TODO
			}			
		}
		
		
		function PopUp(hideOrshow) {
		        if (hideOrshow == 'hide') {
		            document.getElementById('ac-wrapper').style.display = "none";
		            localStorage.setItem("popupWasShown",1);
		            document.getElementById('leftNav').style.top= "135px";
		        }
		        else  if(localStorage.getItem("popupWasShown") == null) {
		            document.getElementById('ac-wrapper').removeAttribute('style');
		            document.getElementById('leftNav').style.top= "230px";
		        }
		    }
		    
		window.onload = function () {
			setTimeout(function () {
		    PopUp('show');	   
	        }, 0);
	    }
		
		    
		function hideNow(e) {
		        if (e.target.id == 'ac-wrapper') document.getElementById('ac-wrapper').style.display = 'none';
		    }
		    		
			
