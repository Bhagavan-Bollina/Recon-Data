	function getGCDMLabel(labelName){
		return $(".gcdmHiddenLabels").find("."+ labelName).text();
	}
	
	function replaceGCDMLabels($formContainer){
		$formContainer.find(".gcdmLabel").each(function(){
			var label = getGCDMLabel($(this).data("labelname"));
			$(this).html(label);
		})
