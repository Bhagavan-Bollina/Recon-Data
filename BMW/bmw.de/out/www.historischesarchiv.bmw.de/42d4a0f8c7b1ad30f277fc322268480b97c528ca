							function getGcdmLanguage() {
								return 'de';
							}

							function getGcdmCountry() {
								return 'DE';
							}

							function setGcdmTextFromPMA(pHTMLPrefix, pClassName, pText, pPMAText, pHTMLPostfix) {
								var theElements = document.getElementsByClassName(pClassName);
								if (theElements.length == 1) {
									var theElement = theElements[0]
									if (theElement.id.substr(0, pClassName.length) == pClassName) {
										theElement.innerHTML = pHTMLPrefix + pText + pPMAText + pHTMLPostfix;
									}
								}
							}

							function hideGcdmElementFromPMA(pClassName) {
								var theElements = document.getElementsByClassName(pClassName);
								// console.log("hideGcdmElementFromPMA: class = " + pClassName + ", theElements.length = " + theElements.length);

								if (theElements.length == 1) {
									var theElement = theElements[0];
									console.log("hideGcdmElementFromPMA: theElement id = " +  theElement.id +  ", innerHTML = " + theElement.innerHTML);
									if (theElement.id.substr(0, pClassName.length) == pClassName) {
										theElement.style.display='none';
									}
								}
							}

							function addCheckboxListenerRegister() {
								var theInputElementRegister = getPolicyCheckbox();
								// console.log("addCheckboxListenerRegister");
								if (theInputElementRegister) {
									// console.log("addCheckboxListenerRegister: addEventListener");
									theInputElementRegister.addEventListener('click', checkConsentsRegister);
								}
							}

							function checkConsentsRegister() {
								// console.log("checkConsentsRegister");
								checkConsentsWithMessage('gcdmRegister', 'confirmContactPolicyRegisterMessage');
							}

							function addCheckboxListenerLogin() {
								var theInputElementLogin = getPolicyCheckbox();
								// console.log("addCheckboxListenerLogin");
								if (theInputElementLogin) {
									// console.log("addCheckboxListenerLogin: addEventListener");
									theInputElementLogin.addEventListener('click', checkConsentsLogin);
								}
							}

							function checkConsentsLogin() {
								// console.log("checkConsentsLogin");
								checkConsentsWithMessage('loginPolicyAgreeBtn', 'confirmContactPolicyLoginMessage');
							}

							function checkConsentsWithMessage(pButton, pConfirmDivId) {
								if (!isConsentsChecked()) {
									showLanguageDependingElement(pConfirmDivId);
									// do lock the button
									lockButton(pButton, true);
									return false;
								} else {
									hideLanguageDependingElement(pConfirmDivId);
									// do unlock the button
									lockButton(pButton, false);
									return true;
								}
							}

							function showLanguageDependingElement(pIdToShow) {
								var pIdToShowWithLanguage = pIdToShow + '_' + getGcdmLanguage();
								// console.log("showLanguageDependingElement: pIdToShowWithLanguage = " + pIdToShowWithLanguage);

								var elemToShow = document.getElementById(pIdToShowWithLanguage);
								if (elemToShow) {
									elemToShow.style.visibility = 'visible';
								}
							}

							function hideLanguageDependingElement(pIdToHide) {
								var pIdToHideWithLanguage = pIdToHide + '_' + getGcdmLanguage();
								// console.log("hideLanguageDependingElement: pIdToHideWithLanguage = " + pIdToHideWithLanguage);

								var elemToHide = document.getElementById(pIdToHideWithLanguage);
								if (elemToHide) {
									elemToHide.style.visibility = 'hidden';
								}
							}

							function isConsentsChecked() {
								var theInputElementConsents = getPolicyCheckbox();
								if (theInputElementConsents) {
									var theInputElementClasslist = theInputElementConsents.classList;
									// console.log("isConsentsChecked: theInputElementClasslist = " + theInputElementClasslist);
									if (theInputElementClasslist.contains('ng-not-empty')) {
										console.log("isConsentsChecked: yes");
										return true;
									}
								}
								console.log("isConsentsChecked: no");
								return false;
							}

							function getPolicyCheckbox() {
								var theInputTags = document.getElementsByTagName('input');
								// console.log("getPolicyCheckbox: theInputTags.length = " + theInputTags.length);
								for (var i = 0; i < theInputTags.length; i++) {
									var theInputTag = theInputTags[i];
									// if (theInputTag.name) {
									// 	console.log("getPolicyCheckbox: " + i + ".theInputTag.name = " + theInputTag.name);
									// }
									if (theInputTag) {
										var theType = theInputTag.type;
										if (theInputTag.type == 'checkbox') {
											console.log("getPolicyCheckbox: checkbox found, name = " + theInputTag.name);
											if (theInputTag.name.substr(0, 6) == 'helper') {
												console.log("getPolicyCheckbox: GCDM policy checkbox (helper...) found!");
												return theInputTag;
											}
										}
									}
								}
								console.log("getPolicyCheckbox: GCDM policy checkbox NOT found!?");
								return null;
							}

							function lockButton(pButtonId, pLock) {
								var theElement = document.getElementById(pButtonId);
								if (theElement) {
									theElement.disabled = pLock;
								}
							}
						</script>

