<script type="text/javascript">

    if( document.location.toString().indexOf("salesforce") < 0 ) {

        var loadedGoogleAnalytics = false;
        var isGoogleAnalyticsDisabled = false;
        var gaNode;

        _gaq.push(function ()
        {
            loadedGoogleAnalytics = true;
        });

        (function ()
        {
            var ga = document.createElement('script');
            ga.type = 'text/javascript';
            ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(ga, s);

            gaNode = ga;

        })();

        setTimeout(function ()
        {
            if( !loadedGoogleAnalytics ) {
                isGoogleAnalyticsDisabled = true;
                gaNode.parentNode.removeChild(gaNode);
            }
        }, 30000);
    }

</script>

<script type="text/javascript">

    function manuallyTrackPageView(category, action, label) {
        if( document.location.toString().indexOf("salesforce") < 0  && !isGoogleAnalyticsDisabled ) {
            _gaq.push(['_trackEvent', category, action, label])
        }
    }

    function manuallyTrackPageViewWithValue(category, action, label, value) {
        if( document.location.toString().indexOf("salesforce") < 0  && !isGoogleAnalyticsDisabled ) {
            _gaq.push(['_trackEvent', category, action, label, value])
        }
    }
</script>

