<script type="text/javascript">
    var isPhoneDevice, isSupportedTouch, isTablet;

    function getTouLang() {
        var supportedTouLangs = ["en","ja"] || [],
            touLang = 'en',
            acceptedLanguages, displayLanguage, userLang;

        if (supportedTouLangs && supportedTouLangs.length > 0) {
            userLang = 'en';

            if (userLang && supportedTouLangs.indexOf(userLang) > -1) {
                return userLang;
            } else {
                acceptedLanguages = '';
                acceptedLanguages = acceptedLanguages && acceptedLanguages.split(',');

                for (var i=0; i < acceptedLanguages.length; i++) {
                    var lang = acceptedLanguages[i];

                    lang = lang.length > 2 ? lang.slice(0, 2) : lang;
                    if (supportedTouLangs.indexOf(lang) > -1) {
                        return lang;
                    }
                }
            }
        }

        return touLang;
    }

</script>
