                    <a href="/account" class="btn btn-link tk-caption btn-create" {{#is page 'create'}} aria-current="page" aria-disabled="true" tabindex="-1" {{else}} aria-disabled="false" {{/is}}>{{createText}}</a>
                </li>
                <li class="item{{#eq page 'faq'}} active{{/if}}">
                    <a href="/faq" class="btn btn-link tk-caption btn-faq" {{#is page 'faq'}} aria-current="page" aria-disabled="true" tabindex="-1" {{else}} aria-disabled="false" {{/is}}>{{faqText}}</a>
                </li>
            </ul>
        </div>
    </div>
</div>

        
