	    function performSaccountClick() {
	    	var frm = document.frm_test;
	    	frm.method = "POST";
	    	frm.actionID.value = "FindEmail";
	    	var lang_ctry = globalLocale.split("_");
	    	frm.languageCode.value = lang_ctry[0];
   	
	    	frm.submit();
	    }

		

	    function checkGlobalLocale()
		{
	    	if (globalLocale == undefined || globalLocale == null || globalLocale == '') {
				if (navigator.languages != undefined){ 
					globalLocale = navigator.languages[0];
				} 
				else{ 
					globalLocale = navigator.language;
				}
				globalLocale = globalLocale.replace('-', '_');
			}	
		}
