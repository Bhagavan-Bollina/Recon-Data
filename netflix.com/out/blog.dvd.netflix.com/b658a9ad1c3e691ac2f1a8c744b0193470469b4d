var header = $('#headerWrapper');
$(window).scroll(function() {
var scroll = $(window).scrollTop();

if (scroll >= 120) {
header.removeClass('solid-background').addClass('trans-background');
} else {
header.removeClass('trans-background').addClass('solid-background');
}
});
});
  
$(document).ready(function() {
    $('#simple-share-buttons a.scrollbar').click(function() {
        window.open($(this).attr('href'),'title', 'width=600, height=480, scrollbars=yes');
        return false;
    });   
    $('#simple-share-buttons a.no-scrollbar').click(function() {
        window.open($(this).attr('href'),'title', 'width=600, height=480, scrollbars=no');
        return false;
    });   
});

