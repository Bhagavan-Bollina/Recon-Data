            <!-- Thumbnail -->
            

  
  <div class="summary-thumbnail-outer-container">
    <a href="/new-dvd-releases/black-trailblazers-in-film-history" class="summary-thumbnail-container sqs-gallery-image-container" data-title="Black Trailblazers of Film History" data-description="">
      <div class="summary-thumbnail img-wrapper" data-animation-role="image">
  

    

        <!-- Main Image -->
        <img data-src="https://static1.squarespace.com/static/55f32473e4b029b54a7228d2/55f35967e4b0942d2206f6a4/5ed84c6f15a38f0504bd66cf/1591892444700/Black-Trailblazers-in-Film-History.jpg" data-image="https://static1.squarespace.com/static/55f32473e4b029b54a7228d2/55f35967e4b0942d2206f6a4/5ed84c6f15a38f0504bd66cf/1591892444700/Black-Trailblazers-in-Film-History.jpg" data-image-dimensions="1200x675" data-image-focal-point="0.5,0.5" alt="Black Trailblazers of Film History"  data-load="false" class="summary-thumbnail-image"/>

    

    

        

        

