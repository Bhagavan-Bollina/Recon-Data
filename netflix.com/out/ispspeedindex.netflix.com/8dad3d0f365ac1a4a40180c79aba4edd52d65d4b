<script>
    $(document).ready(function () {

      var charMap = {
        "à": "a",
        "á": "a",
        "â": "a",
        "é": "e",
        "è": "e",
        "ê": "e",
        "ë": "e",
        "É": "e",
        "ï": "i",
        "î": "i",
        "ô": "o",
        "ö": "o",
        "û": "u",
        "ù": "u",
        "ü": "u",
        "ñ": "n"
      };

      var normalize = function (input) {
        $.each(charMap, function (unnormalizedChar, normalizedChar) {
          var regex = new RegExp(unnormalizedChar, 'gi');
          input = input.replace(regex, normalizedChar);
        });
        return input;
      };


        function customTokenizer(query) {

            var query = normalize(query);

            if (query.toLowerCase() == "us" || query.toLowerCase() == "usa"){
                query = "United States"
            }
            if (query.toLowerCase() == "uk" || query.toLowerCase() == "britain" || query.toLowerCase() == "great britain"  || query.toLowerCase() == "great" ){
                query = "United Kingdom"
            }

          return Bloodhound.tokenizers.whitespace(query);
        }

        function customDatumTokenizer(datum) {
          console.log('datum tokenizer', datum);

          return datum.tokens;
        }

        var links = [
            
