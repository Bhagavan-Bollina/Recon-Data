        var source = new Bloodhound({
          datumTokenizer: customDatumTokenizer,
          queryTokenizer: customTokenizer,
          local: links
        });

        source.initialize();

        $('#locationSearch .typeahead, #headerLocationSearch .typeahead, #bodyLocationSearch .typeahead').typeahead(null, {
          name: 'matched-links',
          displayKey: 'name',
          autoselect: true,
          source: source.ttAdapter(),
          templates: {
            empty: [
              '<div class="empty-message">',
              'No countries matching current query',
              '</div>'
            ].join('\n'),
              suggestion: Handlebars.compile('<p><a href="{{link}}">{{name}}</a></p>')
          }
        }).bind('typeahead:select', function(e, suggestion){
            $(this).val(suggestion.name);
            console.log($(this));
            $(this).parents('form:first').submit();
        });



    /* Cookies message */
    /* Mimics the netflix.com cookies, so a user does not encounter the message more than once */
    var cookie_bar = $('#cookie-disclosure');

    if (Cookies.get('dcsa')){
        //Do nothing
    } else {
        //Show the cookie bar
        cookie_bar.show()
    }

    // If user closes cookie bar, create the cookie
    cookie_bar.find('.close').click(function(){
        //Cookies.set('dcsa', 'customer', { domain: '.netflix.com', expires: Infinity });  // A cookie for netflix.com is not required.
        Cookies.set('dcsa', 'customer', { expires: Infinity });
        cookie_bar.hide();
    });



    });
</script>



