    <script type="text/javascript">

       $(document).ready(function(){
            $(".clickable-row").click(function() {
                window.document.location = $(this).data("href");
            });
            $("[data-asterisk]").click(function(e){
                e.stopPropagation();
                $.scrollTo( $(this).closest('.expandable-body').find('.support-content'), 200, {offset:-50});
            })

            $('a[href=#map_div]').click(function(e){
                e.preventDefault();
                $.scrollTo($(this).attr('href'), 500);
            });
        });

    </script>

