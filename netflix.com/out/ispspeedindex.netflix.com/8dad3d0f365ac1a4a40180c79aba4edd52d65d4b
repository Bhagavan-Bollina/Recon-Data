            // Puerto Rican Override
            , PRI: {
                noIndex: false,
                fillKey: 'INDEX',
                specialMessage: "As a US overseas territory, Puerto Rican providers are detailed on the expanded USA list. Click to view.",
                URL: '/country/us/?small=True',
                countryName: 'Puerto Rico',
            }

            },
            geographyConfig: {
                borderColor: 'rgba(0,0,0,0.3)',
                borderWidth: 0.75,
                highlightOnHover: false,

                popupTemplate: function(geo, data) {

                    console.log(geo.properties, data);

                    if (data) {
                        if (data.specialMessage) {
                            return [
                               "<div class='google-visualization-tooltip "+ geo.id +"'>" +
                                "<h3>" + data.countryName + "</h3>" +
                                "<table class='map-data-table'><thead><tr>" +
                                "<th>" + data.specialMessage + "</th>" +
                                "</tr>" +
                                "</table>" +
                                "</div>"].join('');
                        }
                        else if (data.noIndex == false) {
                            return [
                                "<div class='google-visualization-tooltip "+ geo.id +"'>" +
                                "<h3>" + data.countryName + "</h3>" +
                                "<table class='map-data-table'><thead><tr>" +
                                "<th>Speed</th>" +
                                "<th>Mbps</th>" +
                                "</tr></thead><tbody>" +
                                "<tr class='high'><td>High</td><td>" + data.highSpeed + "</td></tr>" +
                                "<tr class='average'><td>Average</td><td>" + data.averageSpeed + "</td></tr>" +
                                "<tr class='low'><td>Low</td><td>" + data.lowSpeed + "</td></tr>" +
                                "</tbody></table>" +
                                "</div>"].join('');
                        } else if (data.noIndex == true) {
                            return [
                                "<div class='google-visualization-tooltip "+ geo.id +"'>" +
                                "<h3>" + data.countryName + "</h3>" +
                                "<table class='map-data-table'><thead><tr>" +
                                "<th>Not currently indexed</th>" +
                                "</tr>" +
                                "</table>" +
                                "</div>"].join('');
                        }
                    }
                }

            },
            done: function(datamap) {
                datamap.svg.selectAll('.datamaps-subunit').on('click', function(geography, data) {
                    if (map.options.data[geography.id]) {
                        if (map.options.data[geography.id].noIndex == false) {
                            window.location = (map.options.data[geography.id].URL);
                        }
                    }
                });
            }
        });

        window.addEventListener('resize', function() {
            map.resize();
        });

        $('.hot-spot').hover(function(e){
            // Hover in
            //console.log($(this).data());

            var popup = $('<div />').addClass('google-visualization-tooltip').attr('id', $(this).data('cid') );

            popup.css('top', e.pageY+10).css('left', e.pageX+10);

            if ($(this).data('highSpeed')) {
                popup.html(
                        "<h3>" + $(this).data('name') + "</h3>" +
                        "<table class='map-data-table'><thead><tr>" +
                        "<th>Speed</th>" +
                        "<th>Mbps</th>" +
                        "</tr></thead><tbody>" +
                        "<tr class='high'><td>High</td><td>" + $(this).data('highSpeed') + "</td></tr>" +
                        "<tr class='average'><td>Average</td><td>" + $(this).data('averageSpeed') + "</td></tr>" +
                        "<tr class='low'><td>Low</td><td>" + $(this).data('lowSpeed') + "</td></tr>" +
                        "</tbody></table>");
            } else {
                popup.html(
                        "<h3>" + $(this).data('name') + "</h3>" +
                        "<table class='map-data-table'><thead><tr>" +
                        "<th>" + $(this).data('description') + "</th>" +
                        "</tr>" +
                        "</table>" +
                        "</div>"
                )
            }

           $('body').append(popup);

        }, function(e){
            // Hover Out
            $('#'+ $(this).data('cid')).remove();

        });


    </script>















</body>
</html>