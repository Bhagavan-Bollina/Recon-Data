           function showCppGuest(){
             if (document.getElementById('cpplogin').style.display=="none" || document.getElementById('cpplogin').style.display=="")  { 
               document.getElementById('cpplogin').style.display="block";
          }
          else {
            document.getElementById('cpplogin').style.display="none";
          }
        }
