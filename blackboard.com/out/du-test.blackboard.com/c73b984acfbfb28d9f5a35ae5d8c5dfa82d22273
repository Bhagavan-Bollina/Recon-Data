				$('#user_id').focusout(function(e){
					var message = $(this).val();
					var length = message.length; 
					var externaluser = true;
					
					var atttt = new RegExp("@");
					var atdrexel = new RegExp("@drexel");
					
					var atcheck = atttt.test(message);
					var atdrexelcheck = atdrexel.test(message);
					
					if(length == 0){
						return;
					}
					
					if(!atcheck){
						$("#looksllikebox").css("display","block");
						$("#looksllikebox").addClass("slideInUp");
						
					}
					if(atdrexelcheck){
						$("#looksllikebox").css("display","block");
						$("#looksllikebox").addClass("slideInUp");
						
					}
					
				    
            
            
				});
				
				$('#user_id').focusin(function(e){
					$("#looksllikebox").css("display","none");
					$("#looksllikebox").addClass("fadeOutDown");
					});
				
				(function($) {
    $.fn.clickToggle = function(func1, func2) {
        var funcs = [func1, func2];
        this.data('toggleclicked', 0);
        this.click(function() {
            var data = $(this).data();
            var tc = data.toggleclicked;
            $.proxy(funcs[tc], this)();
            data.toggleclicked = (tc + 1) % 2;
        });
        return this;
    };
}(jQuery));
				
