<!DOCTYPE HTML ><script>
function getQueryVariable(variable)
{
       var query = window.location.search.substring(1);
       var vars = query.split("&");
       for (var i=0;i<vars.length;i++) {
               var pair = vars[i].split("=");
               if(pair[0] == variable){return pair[1];}
       }
       return(false);
}
redirectlink = 'https://learn.dcollege.net';
if(getQueryVariable("new_loc")!=false)
             {
             redirectlink = 'https://learn.dcollege.net/webapps/login/?new_loc=' + getQueryVariable("new_loc");
             }
switch(window.location.hostname){
                case "learn.drexel.edu":
                                window.location.replace(redirectlink);
                                break;
               case "learning.drexel.edu":
                                window.location.replace(redirectlink);
                                break;
}



</script>

