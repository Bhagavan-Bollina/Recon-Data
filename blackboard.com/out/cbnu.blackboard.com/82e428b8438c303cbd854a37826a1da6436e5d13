      <script type="text/javascript">
      function loadLoginPage()
      {
    	  if ( top != self )
    	  {
    		  top.location.replace( self.location.href );
    		}
    	  if(document.forms.login.user_id != undefined)
    		{
    		  document.forms.login.user_id.focus();
    		}
    	  setTimeout("triggerScreenreaderAlert()", 500);
      }
      
      function triggerScreenreaderAlert()
      {
    	if ( document.getElementById( 'loginErrorMessage' ) )
  	    {
    	  $( 'loginErrorMessage' ).update( $('loginErrorMessage').innerHTML );
  	    }
      }
            //로그온
      function OnLogon() 
      {
          if (document.forms["form1"].uid.value == "") {
              alert("사용자 ID를 입력하세요");
              return;
          }
          if (document.forms["form1"].pswd.value == "") {
              alert("사용자의 로그온 비밀번호를 입력하세요");
              return;
          }

      //alert(document.forms["form1"].uid.value);
      //alert(document.forms["form1"].pswd.value);
      //alert(document.forms["form1"].reURL.value);
      
          document.forms["form1"].action = "https://eis.cbnu.ac.kr/auth";
          document.forms["form1"].target = "_top";
          document.forms["form1"].submit();
      }
    </script>

  
 
