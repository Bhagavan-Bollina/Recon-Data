    /* DO NOT MODIFY ANYTHING BELOW THIS LINE */
    
      var emailAddress = ""; //Do Not Change This 

      
$(window).bind("load", function() {
  
      
      // Initialization
      var iLabel = 1;  
      var AnyOneSelected = false;
      var EventsSelected = '';
      var TYLink = '';

      for (i = 0; i < SeriesLength; i++) {
        iLabel = i + 1;  
        $('[name=W' + iLabel + '_regID]').val( emailAddress + "-" + EventSeries[i] );
        $('[name=W' + iLabel + '_EventID]').val( EventSeries[i] );        
      }    
        
      // Capture Change
      $( "[name=emailAddress]" ).change(function() {
		  emailAddress = $("input[name=emailAddress]").val();
          for (i = 0; i < SeriesLength; i++) {
            iLabel = i + 1;  
            $('[name=W' + iLabel + '_regID]').val( emailAddress + "-" + EventSeries[i] );
          }          
          
      });      
      
      // Capture Submit
      $( "form" ).submit(function( event ) {
          
          EventsSelected = "?se="
          for (i = 0; i < SeriesLength; i++) {
            if ( $('[name=W' + (i + 1) + '_box]').is(':checked') ) {
                AnyOneSelected = true;
                EventsSelected += EventSeries[i] + "X";
            }  
          }
          if (EventsSelected.charAt(EventsSelected.length - 1) == 'X') {
              EventsSelected = EventsSelected.slice(0, -1);
          }
          EventsSelected = $("[name='c']").val() + EventsSelected;
         
        
          if (AnyOneSelected == false) {
            $("#selectone").css({"display":"block"});
            document.getElementById('FormStart').scrollIntoView();
        	return false;
          } else {
            $("#selectone").css({"display":"none"});
            console.log('Set TY value to: ' + EventsSelected);
            $("[name='c']").val(EventsSelected); 
                       
          }
      });       
        
});
               
  </script> 
    
