<script type="text/javascript">
$(document).ready(function(){

/*$("#tab-1").Class("no-active"); return false;
$("#tab-2").toggleClass("no-active"); return false;*/
  $("#tab-1").click(function(){
  
   
     

      if($("#tab-1").hasClass("no-active") == true){
          $("#tab-1").addClass("active");
          $("#tab-1").removeClass("no-active");
          $("#panel-1").animate({width: 'toggle'});

     }
      if(document.getElementById("panel-2").style.display == "block"){
          $("#panel-2").hide();
           $("#tab-2").removeClass("active"); 
           $("#tab-2").addClass("no-active");  
        }
      
    
    /*$("#tab-1").add("active"); return false;*/
 
  });

   $("#tab-2").click(function(){
  
   
    if($("#tab-2").hasClass("no-active") == true){
          $("#tab-2").addClass("active");
          $("#tab-2").removeClass("no-active");
          $("#panel-2").animate({width: 'toggle'});

      }
      if(document.getElementById("panel-1").style.display == "block"){
          $("#panel-1").hide();
           $("#tab-1").removeClass("active"); 
           $("#tab-1").addClass("no-active");  
        }
      
    
    /*$("#tab-1").add("active"); return false;*/
 
  });
  
   
});
</script>
<script type="text/javascript">
  $(document).ready(function(){
    $("#close-1").click(function(){
      $("#panel-1").hide(600);
      $("#tab-1").removeClass("active");
      $("#tab-1").addClass("no-active");
          
    });
    $("#close-2").click(function(){
      $("#panel-2").hide(600);
      $("#tab-2").removeClass("active");
      $("#tab-2").addClass("no-active");
    });

  });
  
</script>

