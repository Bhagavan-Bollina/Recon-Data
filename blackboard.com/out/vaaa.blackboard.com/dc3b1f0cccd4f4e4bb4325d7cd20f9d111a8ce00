  // Function to display the error message
  // Receives an error number as input which translates to an index of an array where the appropriate error message is stored
  function displayError(errorNumber){
  	//Function that writes out the error message based on the number provided to the function
  	document.getElementById("errorMessage").innerHTML = myErrorMessage[errorNumber];
  	errorWin.style.display = "block";
  }

  // Populates the browser check section with the appropriate values
  (function(){
    // Checks the browser and stores the Browser.Type and Browser.Version in the variable myBrowser
    myBrowser = whichBrowserVersion();
  	// Performs the tests for Blackbaord Learn
  	// Writes out results of OS Test
  	document.getElementById("OSLearn").innerHTML = osCompatibleLearn(whichOS(myBrowser.type));
  	// Writes out results of the Browser Check
  	document.getElementById("browserTestLearn").innerHTML = browserCompatibleLearn(myBrowser);
  	// Writes to the jsTest element if Javascript is enabled
  	document.getElementById("jsTestLearn").innerHTML = "<a href=\"#popup1\" onClick=\"displayError(4);\">" + passImg + "</a>";
  	// Writes to the javaVersion element if a compatible version of Java is installed
  	document.getElementById("javaVersionLearn").innerHTML = javaCheckLearn(myBrowser);

  	// Performs the tests for Blackbaord Collaborate
  	// Writes out results of OS Test
  	document.getElementById("OSCollab").innerHTML = osCompatibleCollab(whichOS(myBrowser.type));
  	// Writes out results of the Browser Check
  	document.getElementById("browserTestCollab").innerHTML = browserCompatibleCollab(myBrowser);
  	// Writes to the jsTest element if Javascript is enabled
  	document.getElementById("jsTestCollab").innerHTML = "<a href=\"#popup1\" onClick=\"displayError(5);\">" + passImg + "</a>";
  	// Writes to the javaVersion element if a compatible version of Java is installed
  	document.getElementById("javaVersionCollab").innerHTML = javaCheckCollab(myBrowser);

  })();

  // Closes the modal window when user clicks the X in the modal window
  span.onclick = function(){
  	errorWin.style.display = "none";
  }

  // This function determines if the browser is supported for Blackboard Learn
  // Receives the browser type and browser version as input
  // It will populate the error message that is displayed when the user clicks the image for this item
  // It will also return the HTML to create the link and display the appropriate image based on whether their browser is supported
  function browserCompatibleLearn(browser){
    // Opera is not a supported browser
    if ( browser.type == "Opera"){
  		myErrorMessage[2] = browser.type + " is not a supported browser for Blackboard Learn. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported browser.</a>";
  		return "<a href=\"#popup1\" onClick=\"displayError(2);\">" + errImg + "</a>";
  	}
    // Internet Explorer is support, but only versions 9 and up
  	else if ( browser.type == "Internet Explorer"){
  		if (browser.version >= learnIELLimit && browser.version <= learnIEULimit){
  			myErrorMessage[2] = browser.type + " " + browser.version +  " is a supported browser for Blackboard Learn.";
  			return "<a href=\"#popup1\" onClick=\"displayError(2);\">" + passImg + "</a>";
  		}
  		else{
  			myErrorMessage[2] = browser.type + " " + browser.version + " is not a supported browser for Blackboard Learn. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported browser.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(2);\">" + errImg + "</a>";
  		}
  	}
    // ESR version 24 and 28 and up of Firefox are supported
  	else if ( browser.type == "Firefox"){
  		if ((browser.version >= learnFirefoxLLimit && browser.version <= learnFirefoxULimit) || browser.version == 24){
  			myErrorMessage[2] = browser.type + " " + browser.version + " is a supported browser for Blackboard Learn.";
  			return "<a href=\"#popup1\" onClick=\"displayError(2);\">" + passImg + "</a>";
  		}
  		else{
  			myErrorMessage[2] = browser.type + " " + browser.version + " is not a supported browser for Blackboard Learn. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported browser.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(2);\">" + errImg + "</a>";
  		}
  	}
    // Version 34 and up of Chrome are supported
  	else if ( browser.type == "Chrome"){
  		if (browser.version >= learnChromeLLimit && browser.version <= learnChromeULimit){
  			myErrorMessage[2] = browser.type + " " + browser.version + " is a supported browser for Blackboard Learn.";
  			return "<a href=\"#popup1\" onClick=\"displayError(2);\">" + passImg + "</a>";
  		}
  		else{
  			myErrorMessage[2] = browser.type + " " + browser.version + " is not a supported browser for Blackboard Learn. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported browser.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(2);\">" + errImg + "</a>";
  		}
  	}
    // Version 6 and up of Safari are supported
  	else if ( browser.type == "Safari"){
  		if (browser.version >= learnSafariLLimit && browser.version <= learnSafariULimit){
  			myErrorMessage[2] = browser.type + " " + browser.version + " is a supported browser for Blackboard Learn.";
  			return "<a href=\"#popup1\" onClick=\"displayError(2);\">" + passImg + "</a>";
  		}
  		else{
  			myErrorMessage[2] = browser.type + " " + browser.version + " is not a supported browser for Blackboard Learn. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported browser.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(2);\">" + errImg + "</a>";
  		}
  	}
    // If the browser is not listed above, then it will show that it's unsupported
  	else{
  		myErrorMessage[2] = browser.type + " " + browser.version + " is not a supported browser for Blackboard Learn. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported browser.</a>";
  		return "<a href=\"#popup1\" onClick=\"displayError(2);\">" + errImg + "</a>";
  	}
  }

  // This function determines if the browser is supported for Blackboard Collaborate
  // Receives the browser type and browser version as input
  // It will populate the error message that is displayed when the user clicks the image for this item
  // It will also return the HTML to create the link and display the appropriate image based on whether their browser is supported
  function browserCompatibleCollab(browser){
    // Opera is not a supported browser
  	if ( browser.type == "Opera"){
  		myErrorMessage[3] = browser.type + " " + browser.version + " is not a supported browser for Blackboard Collaborate. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported browser.</a>";
  		return "<a href=\"#popup1\" onClick=\"displayError(3);\">" + errImg + "</a>";
  	}
    // Internet Explorer is support, but only versions 9 and up
    else if ( browser.type == "Internet Explorer"){
  		if (browser.version >= collabIELLimit && browser.version <= collabIEULimit){
  			myErrorMessage[3] = browser.type + " " + browser.version + " is a supported browser for Blackboard Collaborate.";
  			return "<a href=\"#popup1\" onClick=\"displayError(3);\">" + passImg + "</a>";
  		}
  		else{
  			myErrorMessage[3] = browser.type + " " + browser.version + " is not a supported browser for Blackboard Collaborate. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported browser.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(3);\">" + errImg + "</a>";
  		}
  	}
    // ESR version 24 and 28 and up of Firefox are supported
    else if ( browser.type == "Firefox"){
  		if ((browser.version >= collabFirefoxLLimit && browser.version <= collabFirefoxULimit) || browser.version == 24){
  			myErrorMessage[3] = browser.type + " " + browser.version + " is a supported browser for Blackboard Collaborate.";
  			return "<a href=\"#popup1\" onClick=\"displayError(3);\">" + passImg + "</a>";
  		}
  		else{
  			myErrorMessage[3] = browser.type + " " + browser.version + " is not a supported browser for Blackboard Collaborate. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported browser.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(3);\">" + errImg + "</a>";
  		}
  	}
    // Version 34 and up of Chrome are supported
  	else if ( browser.type == "Chrome"){
  		var currentOS = whichOS(browser.type),
  		    isMac = currentOS.indexOf("Mac");

  		if (isMac < 0 && browser.version >= collabChromeLLimit){
  			myErrorMessage[3] = browser.type + " " + browser.version + " is a supported browser for Blackboard Collaborate.";
  			return "<a href=\"#popup1\" onClick=\"displayError(3);\">" + passImg + "</a>";
  		}
  		else{
  			myErrorMessage[3] = browser.type + " " + browser.version + " is not a supported browser for Blackboard Collaborate. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported browser.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(3);\">" + errImg + "</a>";
  		}
  	}
    // Version 6 and up of Safari are supported
  	else if ( browser.type == "Safari"){
  		if (browser.version >= collabSafariLLimit && browser.version <= collabSafariULimit){
  			myErrorMessage[3] = browser.type + " " + browser.version + " is a supported browser for Blackboard Collaborate.";
  			return "<a href=\"#popup1\" onClick=\"displayError(3);\">" + passImg + "</a>";
  		}
  		else{
  			myErrorMessage[3] = browser.type + " " + browser.version + " is not a supported browser for Blackboard Collaborate. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported browser.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(3);\">" + errImg + "</a>";
  		}
  	}
    // If the browser is not listed above, then it will show that it's unsupported
  	else{
  		myErrorMessage[3] = browser.type + " " + browser.version + " is not a supported browser for Blackboard Collaborate. <a href=\"learn\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported browser.</a>";
  		return "<a href=\"#popup1\" onClick=\"displayError(3);\">" + errImg + "</a>";
  	}
  }
  // This function determines the browser and version that is being used to access Learn
  // It will return an object called browser that contains properties type(string) and version(int)
  function whichBrowserVersion(){
  	// Gets the browser user agent string
  	var ua = navigator.userAgent,
        browser = {type: "N/A", version: 1};

  	if (ua.indexOf("MSIE") != -1){
  			browser.type = "Internet Explorer";
  			browser.version = parseInt(ua.substring(msie+5, ua.indexOf(".", msie)));
  	}
  	else if (ua.indexOf("Trident") != -1 ){
  			var TridentIndex = parseInt(ua.substring(ua.indexOf("Trident")+8, ua.indexOf(".", ua.indexOf("Trident"))));
  			browser.type = "Internet Explorer";
  			browser.version = TridentIndex+4;
  	}
  	else if (ua.indexOf("Opera") != -1 ){
  			browser.type = "Opera";
  			browser.version = parseInt(ua.substring(opera+6, ua.indexOf(".", opera+6)));
  	}
  	else if (ua.indexOf("Firefox") != -1 ){
  			browser.type = "Firefox";
  			browser.version = parseInt(ua.substring(ua.indexOf("Firefox")+8, ua.indexOf(".", ua.indexOf("Firefox"))));
  	}
  	else if (ua.indexOf("Chrome") != -1 ){
  			browser.type = "Chrome";
  			browser.version = parseInt(ua.substring(ua.indexOf("Chrome")+7, ua.indexOf(".", ua.indexOf("Chrome"))));
  	}
  	else if (ua.indexOf("Safari") != -1 ){
  			browser.type = "Safari";
  			browser.version = parseInt(ua.substring(ua.indexOf("Safari")-6, ua.indexOf(".", ua.indexOf("Safari")-6)));
  	}
  	else{
  			browser.type = "Not Known";
  			browser.version = 0;
  	}
  	return browser;
  }

  // This function determines the Operating System using the UserAgent string
  // It accepts input of an object called browser that contains properties type(string) and version(int)
  // It returns a string that contains the OS type and version
  function whichOS(browser){
  	// Gets the browser user agent string and sets the variables returning the index of the OS
  	var ua = navigator.userAgent,
        mac = ua.indexOf ("Mac "),
  	    windows = ua.indexOf("Windows "),
        linux = ua.indexOf("Linux"),
        osVersion,
        tmp;
    // If the Mac string is listed, it gets that information and then replaces the underscore with a period
    if ( mac >= 0){
  		var underScoreIndex = 0,
  		    semicolonIndex = 0;
  		chromeSafarIndex = ua.indexOf (")");
  		semicolonIndex = ua.lastIndexOf ("; ");

  		if (browser == "Firefox"){
  			// Get the version from Firefox
  			tmp = ua.substring(mac, semicolonIndex);
  		}
  		else{
  			// If the browser isn't Firefox, it checks for Chrome or Safari
  			tmp = ua.substring(mac, chromeSafarIndex-2);
  		}
  		// Replaces underscores with periods so that the version string is properly formatted
  		osVersion = tmp.replace(/_/g, ".");
  		return osVersion;
  	}
  	else if ( windows >= 0){
      // If the Window string is listed, it gets that information and then looks up the user friendly version of Windows
  		tmp = ua.substring(windows, ua.indexOf(".", windows)+2);
  		osVersion = windowsLookup(tmp);
  		return osVersion;
  	}
  	else if ( linux >= 0)
  		return "Unsupported Operating System";
  	else
  		return "Unsupported Operating System";
  }
  // This is a function that translates the Windows version in the user agent string to a user recognizable format
  function windowsLookup(OS){
  	if (OS == "Windows NT 10.0")
  		return "Windows 10";
  	else if (OS == "Windows NT 6.3")
  		return "Windows 8.1";
  	else if (OS == "Windows NT 6.2")
  		return "Windows 8";
  	else if (OS == "Windows NT 6.1")
  		return "Windows 7";
  	else if (OS == "Windows NT 6.0")
  		return "Windows Vista";
  	else if (OS == "Windows NT 5.1")
  		return "Windows XP";
  	else if (OS == "Windows NT 5.0 ")
  		return "Windows 2000";
  }
  // This function determines whether the detected Operating System meets the requirements for Blackboard Learn
  function osCompatibleLearn(curOS){
  	// Determines if the version of the Operating System is Mac OS X
  	if (curOS.indexOf("Mac") >= 0)
  	{
  		// Gets the specific Mac version from curOS
  		var macVersion = Number(curOS.substring(curOS.indexOf(".")+1, curOS.length));
  		// Determines if the version of Mac OS X is supported. This includes versions 10.7, 10.8, 10.9 and 10.10
  		if (macVersion > 6 && macVersion <= 10)
  		{
  			myErrorMessage[0] = curOS + " is a supported Operating System for Blackboard Learn.";
  			return "<a href=\"#popup1\" onClick=\"displayError(0);\">" + passImg + "</a>";
  		}
  		else
  		{
  			myErrorMessage[0] = "You are not using a supported Operating System for Blackboard Learn. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported Operating System.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(0);\">" + errImg + "</a>";
  		}
  	}
  	// Determines if the version of the Operating System is Windows
  	else if (curOS.indexOf("Windows") >= 0)
  	{
  		// Gets the specific version of Windows from curOS
  		var winVersion = Number(curOS.substring(curOS.indexOf("Windows")+8, curOS.length));
  		// Second statement to get the specific version if they are using Windows Vista
  		var winVersionString = curOS.substring(curOS.indexOf("Windows")+8, curOS.length);
  		// Determines if the version of Windows is supported. This includes versions Vista, 7, 8 and 8.1
  		if (winVersion >= 7 && winVersion <= 10){
  			myErrorMessage[0] = curOS + " is a supported Operating System for Blackboard Learn.";
  			return "<a href=\"#popup1\" onClick=\"displayError(0);\">" + passImg + "</a>";
  		}
      // Checks to determine if the user is using Vista
  		else if (winVersionString == "Vista"){
  			myErrorMessage[0] = curOS + " is a supported Operating System for Blackboard Learn.";
  			return "<a href=\"#popup1\" onClick=\"displayError(0);\">" + passImg + "</a>";
  		}
      // If it's not a Vista or 7 and up then display the error message
  		else{
  			myErrorMessage[0] = "You are not using a supported Operating System for Blackboard Learn. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported Operating System.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(0);\">" + errImg + "</a>";
  		}
  	}
    // If it's not Mac or Windows then it is unsupported
  	else
  	{
  		myErrorMessage[0] = "You are not using a supported Operating System for Blackboard Learn. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported Operating System.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(0);\">" + errImg + "</a>";
  	}
  }
  // This function determines whether the detected Operating System meets the requirements for Blackboard Collaborate
  function osCompatibleCollab(curOS){
