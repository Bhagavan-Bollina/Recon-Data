  // Learn supports different versions of Java depending on the OS that you are using
  // Receives the browser type as input
  // Gets the Operating System
  function javaCheckLearn(browser){
  	// Determines what version of Java is installed and whether it is a supported by Blackboard Learn
  	// Gets the versions of Java installed that are 1.6 or higher
  	var supportedVersion = deployJava.versionCheck("1.6+"),
        javaVersions = deployJava.getJREs();
  	// Check that determines if any versions of 1.6 are installed, and it returns the appropriate image
  	if (supportedVersion == true){
  		myErrorMessage[6] = javaVersions[0] + " is a supported version of Java for Blackboard Learn.";
  		return "<a href=\"#popup1\" onClick=\"displayError(6);\">" + passImg + "</a>";
  	}
  	else if (javaVersions[0] != undefined){
  		myErrorMessage[6] = javaVersions[0] + " is not a supported version of Java for Blackboard Learn. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with installing a supported version of Java.</a>";
  		return "<a href=\"#popup1\" onClick=\"displayError(6);\">" + errImg + "</a>";
  	}
  	else if (whichOS(browser).indexOf("Mac") >= 0 && browser.type == "Chrome"){
  		myErrorMessage[6] = "Google Chrome does not support Java on Mac OS X 10.7+. Some aspects of Blackboard Learn may not work properly without Java. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported browser and ensuring Java is installed.</a>";
  		return "<a href=\"#popup1\" onClick=\"displayError(6);\">" + errImg + "</a>";
  	}
  	else if (browser.type == "Chrome" && browser.version >= 42){
  		myErrorMessage[6] = "Chrome versions 42+ do not support NPAPI plugins. Some Learn features may not render correctly. If you need these features please use Internet Explorer, Firefox, or <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">click here to contact Technical Support to assist you with installing a supported version of Java.</a>";
  		return "<a href=\"#popup1\" onClick=\"displayError(6);\">" + errImg + "</a>";
  	}
  	else{
  		myErrorMessage[6] = "Java is not installed on this system. Some aspects of Blackboard Learn may not work properly without Java installed. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with installing a supported version of Java.</a>";
  		return "<a href=\"#popup1\" onClick=\"displayError(6);\">" + errImg + "</a>";
  	}
  }
  // Collaborate supports different versions of Java depending on the OS that you are using
  // Receives the browser type as input
  // Gets the Operating System
  function javaCheckCollab(browser){
  	var currentOS = whichOS(browser), // Calls the whichOS() function to get the current Operating System
  	    javaVersions = deployJava.getJREs(), // Gets the Java JRE Version through the browser
  	    isMac = currentOS.indexOf("Mac"); // Checks to see if the OS is Mac OS X
  	// If the OS is Mac OS X it checks the Java versions
  	if (isMac >= 0){
  		// If the version of Java is greater than or equal to 1.7 but less than 1.8 since 1.8 is not supported for Mac OS X
  		if (deployJava.versionCheck("1.7") == true && deployJava.versionCheck("1.8") == false){
  			myErrorMessage[7] = javaVersions[0] + " is a supported version of Java for Blackboard Collaborate.";
  			return "<a href=\"#popup1\" onClick=\"displayError(7);\">" + passImg + "</a>";
  		}
  		else if (deployJava.versionCheck("1.8") == true){
  			myErrorMessage[7] = "Java 1.8 is installed, which is not currently supported for Collaborate. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with installing a supported version of Java.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(7);\">" + errImg + "</a>";
  		}
  		else if (deployJava.versionCheck("1.6") == true){
  			myErrorMessage[7] = "Java 1.6 is installed, which is not currently supported for Collaborate. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with installing a supported version of Java.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(7);\">" + errImg + "</a>";
  		}
  		else if (browser.type == "Chrome"){
  			myErrorMessage[7] = "Google Chrome does not support Java on Mac OS X 10.7+. We recommend using another browser to ensure Java is working properly. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with finding a supported browser and ensuring Java is installed.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(7);\">" + errImg + "</a>";
  		}
  		else{
  			myErrorMessage[7] = "Java is not installed on this system, which is needed for Blackboard Collaborate. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with installing a supported version of Java.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(7);\">" + errImg + "</a>";
  		}
  	}
  	// If the version is not a Mac, then it checks to ensure that Java 1.7 or higher is installed
  	else{
  		if (browser.type == "Chrome" && browser.version >= 42){
  				myErrorMessage[6] = "Chrome versions 42+ do not support NPAPI plugins. If you know that Java is installed on your system you may proceed. If you are unsure whether Java is installed, please <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">click here to contact Technical Support to assist you with installing a supported version of Java.</a>";
  				return "<a href=\"#popup1\" onClick=\"displayError(6);\">" + errImg + "</a>";
  		}
  		else if (deployJava.versionCheck("1.7+") == true){
  			myErrorMessage[7] = javaVersions[0] + " is a supported version of Java for Blackboard Collaborate.";
  			return "<a href=\"#popup1\" onClick=\"displayError(7);\">" + passImg + "";
  		}
  		else if (deployJava.versionCheck("1.6") == true){
  			myErrorMessage[7] = "Java 1.6 is installed, which is not currently supported for Blackboard Collaborate. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support to assist you with installing a supported version of Java.</a>";
  			return "<a href=\"#popup1\" onClick=\"displayError(7);\">" + errImg + "";
  		}
      else {
        // If not Java is detected it returns the negative image to show this
        myErrorMessage[7] = "Java was not detected on your system, which is needed for Blackboard Collaborate. <a href=\"http://acquisitionacademy.edusupportcenter.com/sims/helpcenter/common/layout/SelfHelpHome.seam?inst_name=vaaa\" target=\"_new\">Click here to contact Technical Support.</a>";
        return "<a href=\"#popup1\" onClick=\"displayError(7);\">" + errImg + "";
      }
  	}
  }
