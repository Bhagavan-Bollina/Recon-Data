  <script type="text/javascript">
      function loadLoginPage()
      {

      	inicio();

    	  if ( top != self )
    	  {
    		  top.location.replace( self.location.href );
    		}
    	  if(document.forms.login.user_id != undefined)
    		{
    		  document.forms.login.user_id.focus();
    		}
    	  setTimeout("triggerScreenreaderAlert()", 500);
      }
      
      function triggerScreenreaderAlert()
      {
    	if ( document.getElementById( 'loginErrorMessage' ) )
  	    {
    	  $( 'loginErrorMessage' ).update( $('loginErrorMessage').innerHTML );
  	    }
      }
    </script>
    <script type="text/javascript">
	    function login_openForgotPassword( url ) {   
		    var passwordWin = window.open( url, 'forgotPasswordWindow', 'menubar=1,resizable=1,scrollbars=1,status=1,width=850,height=480' );
		    passwordWin.focus();
	    }
    </script>
    <script type="text/javascript">
	    function onBlur(el) {
		    if (el.value == '') {
			    el.value = el.defaultValue;
		    }
	    }
	    function onFocus(el) {   
		    if (el.value == el.defaultValue) {
			    el.value = '';
		    }
	    }
	function validateInput(form) {
		form.username.value = form.username.value.replace(/^\s*|\s*$/g,"");
		if ( form.username.value == "" || form.password.value == "" ) {

			alert( "Enter a username and password." );

			return false;

		}
		else
			return true;
	}

	reDomainId = /^(([a-zA-Z]{3}[0-9]{1})|(laur)|(bb)|(lnps)|(iede))\./;

	function appendDomainID() {
		formRef = document.getElementById("login");
		ck = validateInput( formRef );
		if ( ck == true && formRef.username.value != "" ) {
			if ( reDomainId.test(formRef.username.value) == false ) {
				formRef.user_id.value = formRef.username.value;
				return true;
			}
			else {
				formRef.user_id.value = formRef.username.value;
				return true;
			}
		}
		else {
			return false;
		}
	}	
    </script>
