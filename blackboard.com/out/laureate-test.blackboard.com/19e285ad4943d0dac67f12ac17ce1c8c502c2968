    <script type="text/javascript">
    	function inicio(){
            if (getURLParameter('acronym') == 'intlclass'){
                
                    var cssLink = document.createElement('link');
            		cssLink.rel = 'stylesheet';
            		cssLink.type = 'text/css';
            		cssLink.href = '/bbcswebdav/institution/login/LOGIN_PAGE/css/login.css';
            
            		document.getElementsByTagName('head')[0].appendChild(cssLink);

                    var cssSlider = document.createElement('link');
                    cssSlider.rel = 'stylesheet';
                    cssSlider.type = 'text/css';
                    cssSlider.href = '/bbcswebdav/institution/login/LOGIN_PAGE/css/imgslider.css';
            
                    document.getElementsByTagName('head')[0].appendChild(cssSlider);

                    var mc = document.getElementById('main-container');
                    mc.style.display = '';

                    var btnLogin = document.getElementById('btnLogin');
                    btnLogin.value = 'Ingresar';

                    var lh = document.getElementById('login_help');
                    lh.href = 'mailto:international.class@laureate.net';
                    lh.innerHTML = 'Ayuda para Ingresar';

                    document.getElementById('login').setAttribute('action','/webapps/login/?acronym=intlclass');

                    var usrnm = document.getElementById('username');
                    usrnm.value = 'nombre de usuario';
                    usrnm.onfocus = function(){
                                        usrnm.value = '';
                                    };
                    usrnm.onblur = function(){
                                        if (usrnm.value == ''){
                                            usrnm.value = 'nombre de usuario';
                                        }
                                    };

                    var lostPass = document.getElementById('lostPass');
                    lostPass.title = 'Lost Password?';
                    lostPass.innerHTML = 'Olvid\xF3 su contrase\xF1a?';

                    var loginError = document.getElementById('loginErrorMessage');

                    if (loginError !== null){
                        loginError.innerHTML = 'El nombre de usuario o contrase\xF1a es incorrecto. Por favor intente de nuevo.';
                    }

                    <!--SCRIPTS NO COMPATIBLES EN IE-->
                    if (navigator.appName == 'Microsoft Internet Explorer'){
                        document.title = 'Aulas Internacionales - Laureate International Universities';
                    }
                    else{
                        document.getElementsByTagName('title')[0].innerHTML='Aula Internacional - Laureate International Universities';
                    }
            } else {
                var cssLinkOriginal = document.createElement('link');
                cssLinkOriginal.rel = 'stylesheet';
                cssLinkOriginal.type = 'text/css';
                cssLinkOriginal.href = '/branding/themes/lls/loginbb.css';

                document.getElementsByTagName('head')[0].appendChild(cssLinkOriginal);
                
                document.getElementsByName('logo')[0].src = '/branding/themes/lls/images/logo.png';

                var mc = document.getElementById('main-container');
                mc.style.display = '';
                mc.style.backgroundColor = 'transparent';
                mc.style.marginLeft = '0px';
                mc.style.paddingLeft = '0px';

                var atp = document.getElementById('abouttheprogram');
                atp.style.display = 'none';

                var pinstr = document.getElementById('instr');
                pinstr.style.display = 'none';

                <!--SCRIPTS NO COMPATIBLES CON IE-->
                if (navigator.appName == 'Microsoft Internet Explorer'){
                    document.title = 'Blackboard Learn';
                }
                else{
                    document.getElementsByTagName('title')[0].innerHTML = 'Blackboard Learn';
                }
            }
        }            

        function getURLParameter(myParameterName) {
            var sURL = document.URL.toString();
            
            if (sURL.indexOf("?") > 0) {
                var arrParams = sURL.split("?");
                    
                var arrURLParams = arrParams[1].split("&");
                
                var arrParamNames = new Array(arrURLParams.length);
                var arrParamValues = new Array(arrURLParams.length);
                
                var i = 0;
                for (i=0; i < arrURLParams.length; i++) {
                    var sParam =  arrURLParams[i].split("=");
                    arrParamNames[i] = sParam[0];
                    if (sParam[1] != "")
                        arrParamValues[i] = unescape(sParam[1]);
                    else
                        arrParamValues[i] = "";
                }
                
                for (i=0; i < arrURLParams.length; i++) {
                    if (arrParamNames[i] == myParameterName) {
                        return arrParamValues[i]
                    }
                }
            } else {
                return "";
            }
        }
    </script>
  
 
