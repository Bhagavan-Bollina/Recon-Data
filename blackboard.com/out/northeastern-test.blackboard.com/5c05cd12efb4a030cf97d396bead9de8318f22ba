    <script type="application/javascript">
      $.urlParam = function(name){
        var results = new RegExp('[\?&]' + name + '=([^&#]*)').exec(window.location.href);
        if (results==null){
          return null;
        }
        else{
          return decodeURI(results[1]) || 0;
        }
      }

      $.noConflict();
      jQuery(document).ready(function ($) {
        // Get the query portion of the URL
        if ($.urlParam('new_loc')) {
          var new_loc = $.urlParam('new_loc');
        } else {
          var new_loc = "";
        }
        console.log("new_loc = " + new_loc);

        // PROD
        document.getElementById('ssoUrl').href = "https://northeastern-test.blackboard.com/auth-saml/saml/login?apId=_102_1&redirectUrl=https%3A%2F%2Fnortheastern-test.blackboard.com" + new_loc;
        // STAGING
        // document.getElementById('ssoUrl').href = "https://northeastern-staging.blackboard.com/auth-saml/saml/login?apId=_5355_1&redirectUrl=https%3A%2F%2Fnortheastern-staging.blackboard.com" + new_loc;
        // TEST
        //document.getElementById('ssoUrl').href = "https://northeastern-test.blackboard.com/auth-saml/saml/login?apId=_102_1&redirectUrl=https%3A%2F%2Fnortheastern-test.blackboard.com" + new_loc;
        
        document.getElementById("default_login_link").href = "/webapps/login/?action=default_login&new_loc="  + new_loc;
      });
    </script>
  
 
