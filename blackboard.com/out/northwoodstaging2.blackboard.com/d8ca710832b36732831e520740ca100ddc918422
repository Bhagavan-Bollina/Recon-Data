	<script type="text/javascript">
	 
	 function loadLoginPage()
	 {
			if ( top != self )
			{
				top.location.replace( self.location.href );
			}
			if(document.forms.login.user_id != undefined)
			{
				document.forms.login.user_id.focus();
			}
			setTimeout("triggerScreenreaderAlert()", 500);
	 }
	 
	 function triggerScreenreaderAlert()
	 {
		if ( document.getElementById( 'loginErrorMessage' ) )
			{
			jQuery( 'loginErrorMessage' ).update( jQuery('loginErrorMessage').innerHTML );
			}
	 }
	</script>
<script type="text/javascript">
		window.onload = function() {
			var loginForm = document.querySelector('form[name=login]');
			loginForm.remove();
			var loginError = document.getElementById("loginErrorMessage");
			if(loginError.innerHTML=="You've been logged out due to inactivity. Log in again to continue.")
			{	loginError.innerHTML="";
				loginError.style.visibility="hidden";
			}
		} 
	</script>


