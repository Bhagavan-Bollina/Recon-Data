	jQuery('input[name=user_id]').val(jQuery('input[name=NUNOW_user_id]').val());
	jQuery('input[name=password]').val(jQuery('input[name=NUNOW_password]').val());
	jQuery('input[name=new_loc]').val('/webapps/nu-mir3-bb_bb60/module/mir3Reg.jsp');
	jQuery('form[name=login]').submit();
}

