	<script type="text/javascript">
		jQuery(document).ready(function(){
			var width = jQuery(document).width();

			//If we are in a frame, hide the tab header
			if(window.top !== window.self) {
				// embedded
				jQuery('#tabheader').addClass('hide-on-desktop');
				jQuery('#tabheader').addClass('hide-on-tablet');
				jQuery('div[class=main]').addClass('noheader');
			}
			// Load NU NOW dialog on click
			jQuery('#basic-modal .basic').click(function (e) {
				jQuery('#basic-modal-content').modal({overlayClose:true});
				return false;
			});
	
			//Battle the demons of the bb login widget
			jQuery('#loginFormList>li').removeClass('clearfix');
			jQuery('input[type="submit"], button[type="submit"]').removeClass('submit');
			jQuery('input[type="submit"], button[type="submit"]').removeClass('button-1');
			jQuery('input[type="submit"], button[type="submit"]').addClass('btn submitbtn');
			
			
			jQuery('input[type="submit"], button[type="submit"]').css('display','none');
			//jQuery('#user_id, #password').addClass('logininput');
			//jQuery('#user_id, #password').parent().addClass('grid-50');
			//jQuery('#user_id, #password').parent().addClass('mobile-grid-100');
			//jQuery('#user_id, #password').parent().addClass('tablet-grid-100');
			//jQuery('#user_id, #password').parent().css('padding-left','10px');
			//jQuery('#user_id, #password').parent().css('padding-right','10px');
			jQuery('#user_id, #password').parent().css('display','none');
			
			jQuery('#redirectProvidersDropdownButton').css('display','none');
			
			jQuery('#loginRedirectProviderList').css('display','block !important');
			jQuery('#loginRedirectProviderList>li>a').addClass('button expand btn submitbtn');
			jQuery('#loginRedirectProviderList>li').css('width','100%');
			
			
			jQuery('input[type="submit"], button[type="submit"]').parent().prepend('<!-- <p class="aup"></p> -->');
			//Remove @northwood.edu from userid
			jQuery('#user_id').on('blur', function(){
			 jQuery('#user_id').val(jQuery('#user_id').val().replace("\@northwood.edu", ""));
			});

			//START ALERT NOTICES
			jQuery.ajax({
				url: '//nusites.northwood.edu/webapps/alerts/jsonptest.aspx?callback=?',
				type: 'GET',
				async: false,
				jsonpCallback: 'alertCallback',
				contentType: "application/json",
				dataType: 'jsonp',
				success: function(result) {
					//alert(JSON.stringify(result));
					console.log('success loading alerts');
					if(JSON.stringify(result) != "[]") {
					jQuery("#emergency_alerts").removeClass('hidden');
					}
					jQuery.each(result, function(key){
						jQuery("#emergency_alerts").append('<div class="grid-100 tablet-grid-100 mobile-grid-100 mb10"><h2>'+ result[key].msg_title +'</h2> - ' + result[key].msg_body + ' <a href="http://nusites.northwood.edu/webapps/alerts/?msg_id=' + result[key].msg_id + '" target="_blank"><em>Details...</em></a></div>');
					});
				},
				error: function(e) {
					console.log(e.message);
				}
			});
			//END ALERT NOTICES
			//START REMOVE INACTIVITY LOGIN ERROR
			if(jQuery.trim(jQuery("#loginErrorMessage").text()) == jQuery.trim("You've been logged out due to inactivity. Log in again to continue.")){
				jQuery('#loginErrorMessage').hide();
			}
			//END REMOVE INACTIVITY LOGIN ERROR
		});



