  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.17.1/moment-with-locales.min.js"></script>
<script type="text/javascript" src="https://cdn.statuspage.io/se-v2.js"></script>
<script>
	var mDone = false;
	var amDone = false;
	var iDone = false;
	var sp = new StatusPage.page({ page : 'dpd0rtb0h6k9' });
	// Get conglomerate component statuses
	sp.summary({
		success: function(data) {
			// adds the text description to the dropdown
			jQuery('.color-description').text("Northwood University System Status - " + data.status.description);
			// appends the status indicator as a class name so we can use the right color for the status light thing
			jQuery('.color-dot').addClass(data.status.indicator);
		}
	});
	// Incidents

    sp.incidents({
        filter: 'unresolved',
        success: function(data) {
            var i;
            for (i = 0; i < data.incidents.length; i++) {
                var outage = data.incidents[i];
                var outage_name = outage.name;
                var outage_impact = outage.impact;
				var outage_link = outage.shortlink;
				var outage_start = outage.created_at;
				var outage_end = outage.resolved_at;
				var outage_updated = outage.updated_at;
				var outage_details = "";
				var outage_comps = [];
				for (var oc = 0; oc < outage.components.length; oc++) {
					if (outage.components[oc].status != "operational") {
						outage_comps.push(outage.components[oc].name);
					}
				}
				outage_comps.sort(function(a,b){
					return a.toLowerCase().localeCompare(b.toLowerCase());
				});

				var x;
				for (x = 0; x < outage.incident_updates.length; x++) {
					if (x + 1 == outage.incident_updates.length) {
							outage_details = outage_details + "<p><span class='update-status' style='font-weight:bold;text-transform:capitalize;'>" + outage.incident_updates[x].status + "</span> - " + outage.incident_updates[x].body + "</p>";
					}
					else {
							outage_details = outage_details + "<p><span class='update-status' style='font-weight:bold;text-transform:capitalize;'>Update</span> - " + outage.incident_updates[x].body + "</p>";
					}
				}
				var comps_string = "<p>Affected components: </p><ul style='list-style:initial;list-style-position:inside;margin-bottom:1rem;font-size:12px;' class='outage-comps'>";
				for (var c = 0; c < outage_comps.length; c++) {
					comps_string += "<li>" + outage_comps[c] + "</li>";
				}
				comps_string += "</ul>";
				if (outage_updated != outage_start) {
					var outage_string = "<div class='status-outage "+outage_impact+"'><strong><span class='name'>" + outage_name + "</span></strong> - Last updated " + moment(outage_updated).format('MM/DD/YYYY h:mm a') + " - Began <span class='start'>"+moment(outage_start).format('MM/DD/YYYY h:mm a')+"</span><div class='details'>"+comps_string+outage_details+"</div></div>";
				}
				else {
					var outage_string = "<div class='status-outage "+outage_impact+"'><strong><span class='name'>" + outage_name + " - Began <span class='start'>"+moment(outage_start).format('MM/DD/YYYY h:mm a')+"</span><div class='details'>"+outage_details+"</div></div>";
				}
				jQuery('.status-incidents').prepend(outage_string);
			}
			iDone = true;
		}
    });

	// Active Maintenances
	sp.scheduled_maintenances({
		filter: 'active',
		success: function(data) {
			var i;
			for (i = 0; i < data.scheduled_maintenances.length; i++) {
				var outage = data.scheduled_maintenances[i];
				console.log(outage);
				var outage_name = outage.name;
				var outage_link = outage.shortlink;
				var outage_start = outage.scheduled_for;
				var outage_end = outage.scheduled_until;
				var outage_details = "";
				var x;
				for (x = 0; x < outage.incident_updates.length; x++) {
					outage_details = outage_details + "" + outage.incident_updates[x].body + "<br>";
				}
				var outage_string = "<div class='status-outage maintenance'><strong><span class='name'>" + outage_name + "</span></strong> - <span class='start'>"+moment(outage_start).format('MM/DD/YYYY h:mm a')+"</span><span class='to'> to </span><span class='end'>"+moment(outage_end).format('MM/DD/YYYY h:mm a')+"</span><div class='details'>"+outage_details+"</div></div>";
				// console.log(outage_string);
				var start = moment(outage_start);
				var end = moment(outage_end);
				jQuery('.status-maintenances').prepend(outage_string);
				console.log('maintenance added');
			}
			amDone = true;
			console.log('mDone set to true');
		}
	});
	// Upcoming
	sp.scheduled_maintenances({
		filter: 'upcoming',
		success: function(data) {
			var i;
			for (i = 0; i < data.scheduled_maintenances.length; i++) {
				var outage = data.scheduled_maintenances[i];
				console.log(outage);
				var outage_name = outage.name;
				var outage_link = outage.shortlink;
				var outage_start = outage.scheduled_for;
				var outage_end = outage.scheduled_until;
				var outage_details = "";
				var x;
				for (x = 0; x < outage.incident_updates.length; x++) {
					outage_details = outage_details + "" + outage.incident_updates[x].body + "<br>";
				}
				var outage_string = "<div class='status-outage'><strong><span class='name'>" + outage_name + "</span></strong> - <span class='start'>"+moment(outage_start).format('MM/DD/YYYY h:mm a')+"</span><span class='to'> to </span><span class='end'>"+moment(outage_end).format('MM/DD/YYYY h:mm a')+"</span><div class='details'>"+outage_details+"</div></div>";
				// console.log(outage_string);
				var start = moment(outage_start);
				var end = moment(outage_end);
				if(end.isBetween(moment(), moment().add(7, 'days'))){
					jQuery('.status-maintenances').append(outage_string);
				}
				console.log('maintenance added');
			}
			mDone = true;
			console.log('mDone set to true');
		}
	});
	

	var outageClickInterval = setInterval(function(){
		if(mDone === true && iDone === true && amDone === true){
			jQuery('.status-outage').delay(400).click(function(event) {
				jQuery(this).toggleClass('open',(!jQuery(this).hasClass('open')));
				event.preventDefault();
				return false;
			});
			clearInterval(outageClickInterval);
		}
	}, 500);
</script>

