<script language="JavaScript1.2">
	var howOften = 3; //number often in seconds to rotate
	var current = 0; //start the counter at 0
	var timer;
	var ns6 = document.getElementById&&!document.all; //detect netscape 6
	
	// place your images, text, etc in the array elements here
	var items = new Array();
		items[0]="<a href='http://www.northwood.edu/it/my-northwood-mobile' target='_blank'><img alt='My Northwood Mobile' src='https://nusites.northwood.edu/bbfiles/MYNORTHWOOD.png' style='border: 1px black solid;' onMouseover='pauseImg();' onMouseout='playImg();' /></a>";
		items[1]="<a href='https://help.blackboard.com/Mobile_Learn' target='_blank'><img alt='Blackboard Learn' src='https://nusites.northwood.edu/bbfiles/BB.png' onMouseover='pauseImg();' style='border: 1px black solid;' onMouseout='playImg();' /></a>";
		items[2]="<a href='https://www.northwood.edu/campus-life/security/northwood-now' target='_blank'><img alt='Rave Guardian' src='https://nusites.northwood.edu/bbfiles/RAVE.png' style='border: 1px black solid;' onMouseover='pauseImg();' onMouseout='playImg();' /></a>";
		items[3]="<a href='https://northwood.teamdynamix.com/TDClient/KB/ArticleDet?ID=30162' target='_blank'><img alt='Office 365' src='https://nusites.northwood.edu/bbfiles/office365c.png' style='border: 1px black solid;' onMouseover='pauseImg();' onMouseout='playImg();' /></a>";
		items[4]="<a href='https://getrave.com/login/northwood' target='_blank'><img alt='Northwood NOW! Emergency Alert System' src='https://nusites.northwood.edu/bbfiles/NORTHWOODNOW.png' style='border: 1px black solid;' onMouseover='pauseImg();' onMouseout='playImg();' /></a>";
	//function rotater() {
	//	document.getElementById("imgRotator").innerHTML = items[current];
	//	current = (current==items.length-1) ? 0 : current + 1;
	//	setTimeout("rotater()",howOften*1000);
	//}
	
	function rotater() {
		if(document.layers) {
			document.imgRotatorlayer.document.write(items[current]);
			document.imgRotatorlayer.document.close();
		}
		if(ns6)document.getElementById("imgRotatordiv").innerHTML=items[current]
			if(document.all)
				imgRotatordiv.innerHTML=items[current];
	
		current = (current==items.length-1) ? 0 : current + 1; //increment or reset
		timer=setTimeout("rotater()",howOften*1000);
	}
	function pauseImg() {
		clearTimeout(timer);
	}
	function playImg() {
		rotater();
	}
	window.onload=rotater;
