<!-- Vue -->   
<script src="https://unpkg.com/vue"></script>
<!-- Axios -->
<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="https://unpkg.com/vue-toasted"></script>
<script src="https://unpkg.com/moment"></script>
<script>


  Vue.use(Toasted);
  
  var app = new Vue({
    el: '#developer',
    data: {
      messages: null,
      mostrar: '',
      error:'',
      isModalActive: false,
      isModalActive2: true,
      o365: false,
      stop: '',
    },
    mounted() {
      this.getAll();
    },
    methods: {
      getAll(){
        axios.get('https://dae.datanetwork.cl/api/mensaje')
          .then( response => {
            this.messages = response.data;
            console.log('aqui toma la api');
            console.log(this.messages.length);
            
            var dia = new Date();
            // 2019-08-26
            if(dia.getMonth() +1 < 10){
              //var fecha = dia.getFullYear() +"-0"+ (dia.getMonth() +1) + "-" + dia.getDate()
              var fecha = dia.getFullYear() +"-0"+ (dia.getMonth() +1)
            }else{
              //var fecha = dia.getFullYear() +"-"+ (dia.getMonth() +1) + "-" + dia.getDate()
              var fecha = dia.getFullYear() +"-"+ (dia.getMonth() +1)
            }
            if(dia.getDate() < 10){
              var fecha = fecha + "-0" + dia.getDate()
            }else{
              var fecha = fecha + "-" + dia.getDate()
            }
            var fecha1 = moment('2020-04-30');
            var fecha2 = moment();
            var alarmas = moment().format('dddd');
            console.log(alarmas);
            var activar = 0;
            
            this.stop = fecha1.diff(fecha2, 'days');

            for (i = 0; i < this.messages.length; i++) {
                if(this.messages[i]['mensajes'] != undefined ){
                  if( alarmas === 'Monday' && this.messages[i]['lunes'] == 1 )
                  {
                    activar = 1;
                  }
                  else if( alarmas === 'Tuesday' && this.messages[i]['martes'] == 1)
                  {
                    activar = 1;
                  }
                  else if( alarmas === 'Wednesday' && this.messages[i]['miercoles'] == 1 )
                  {
                    activar = 1;
                  }
                  else if( alarmas === 'Sunday' && this.messages[i]['jueves'] == 1)
                  {
                    activar = 1;
                  }
                  else if( alarmas === 'Friday' && this.messages[i]['viernes'] == 1)
                  {
                    activar = 1;
                  }
                  else if( alarmas === 'Saturday' && this.messages[i]['sabado'] == 1)
                  {
                    activar = 1;
                  }
                  else if( alarmas === 'Sunday' && this.messages[i]['domingo'] == 1)
                  {
                    activar = 1;
                  }
                  else if (this.messages[i]['alarma'] === fecha)
                  {
                    activar = 1;
                  }
                  if( activar == 1){
                    console.log(this.messages[i]['tipo'])
                    console.log(this.messages[i]['mensajes']);
                    if( this.messages[i]['tipo'] === "Mensaje" )
                    {
                      this.mostrar = this.messages[i]['mensajes'];
                      this.isModalActive = true; 
                      this.isModalActive2 = false; 
                    }
                    if( this.messages[i]['tipo'] === "informaci\u00f3n" || this.messages[i]['tipo'] === "informaciÃ³n")
                    {
                      this.$toasted.success(this.messages[i]['mensajes'], {  
                          position: "bottom-center", 
                          duration : null,
                          action : {
                                text : 'Cerrar',
                                onClick : (e, toastObject) => {
                                    toastObject.goAway(0);
                                }
                            },
                        });
                    }
                  }
                };
              };
          //}
          })
          .catch( e => console.log(e) )
      }
    },
  })
  </script>