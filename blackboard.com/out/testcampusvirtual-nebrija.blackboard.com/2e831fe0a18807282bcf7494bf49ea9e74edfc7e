            <script language="JavaScript"> 
                var myDate = new Date(); 
                
                
                /* hour is before noon */
                if ( myDate.getHours() < 14 ) 
                { 
                document.write("<h2 class='dias'>&iexcl;Buenos d&iacute;as!</h2>"); 
                } 
                else /* Hour is from noon to 5pm (actually to 5:59 pm) */
                if ( myDate.getHours() >= 14 && myDate.getHours() <= 20 ) 
                { 
                document.write("<h2 class='tardes'>&iexcl;Buenas tardes!</h2>"); 
                } 
                else /* the hour is after 5pm, so it is between 6pm and midnight */
                if ( myDate.getHours() > 20 && myDate.getHours() <= 24 ) 
                { 
                document.write("<h2 class='noches'>&iexcl;Buenas noches!</h2>"); 
                } 
                else /* the hour is not between 0 and 24, so something is wrong */
                { 
                document.write("<h2>&iexcl;Hola!</h2>"); 
                } 
                
                /* remove the following in your live script. It is for testing 
                document.write("<p> Son las: " + myDate.getHours() +"</p>"); */
                
            </script> 

          </div>

         
        <!-- FIN SALUDO -->

          


