  <script>$.noConflict();</script>
    <script type="text/javascript">      
      jQuery(document).ready(function($){
        $('#portal_login').leanModal({ top: 110, overlay: 0.45, closeButton: ".hidemodal" });
        $('#portal_id_list').leanModal({ top: 110, overlay: 0.45, closeButton: ".hidemodal" }); 

        $('#portal').click(function(e){            
            var userId = $('#portal_username').val();
            var passwd =encodeURIComponent($('#portal_password').val());
            var contents = null;            

            if(isNull(passwd)) return false;
            if(isNull(userId)) return false;
 
            $.ajax({
              dataType: 'jsonp'            
              ,type: 'POST'
              ,url: "https://my.dgist.ac.kr/com/member/loginSaasBlackBoard.do"
              ,jsonpCallback: "BlackBoard"
              ,data: {password:passwd, username:userId} 
              ,success: function(data){                
                if (undefined!=data && data.length > 0) {                  

                  if(data.length == 1){
                    //window.location = "/webapps/bbgs-autosignon-BBLEARN/B2login.jsp?userId=" + data[0].STTS_NO;
                    //document.atuoSingOnForm.userId.value=data[0].STTS_NO;
                    document.atuoSingOnForm.action=data[0].returnURL;
                    document.atuoSingOnForm.submit();
                    return false;
                  }

                  $("#list-group").empty(); 
                  
                  for (var i = 0; i < data.length; i++) {                  
                    contents = "<li class='list-group-item' onclick=selectedLogin('"+data[i].returnURL+"')>"
                      +data[i].POSI_DEPT_CD_NM + "  -  "
                      +data[i].KOR_REL_PSN_NM + "  -  "
                      +data[i].STTS_NO 
                      +"</li>"
                    
                    $("#list-group").append(contents);                                      
                  };

                  $("#portal_id_list").click();
                } else {                  
                  alert("로그인 정보가 올바르지 않습니다.");
                }
              }
              ,error: function(request, status, error){                
                alert("로그인 정보가 올바르지 않습니다.." + error);
              }
            });            
        });
        
        // 2016-02-18 추가
        $('#lean_overlay').click(function(e){
          $("#list-group").empty();
        });

        var closeModal = function(id){
          $('#lean_overlay').hide();     
          $('#'+id).hide();      
        };      



      });   

   /*   function logintest(){

        console.log("js ::"+encodeURIComponent(document.getElementById("portal_password").value));

      }
     */ 
      function isNull(obj){
         return (typeof obj != "undefined" && obj!=null && obj !="" &&obj !="0") ? false : true ;
      }

      function loadLoginPage()
      {
        if ( top != self )
        {
          top.location.replace( self.location.href );
        }
        if(document.forms.login.user_id != undefined)
        {
          document.forms.login.user_id.focus();
        }
        setTimeout("triggerScreenreaderAlert()", 500);
      }
      
      function triggerScreenreaderAlert()
      {
      if ( document.getElementById( 'loginErrorMessage' ) )
        {
        $( 'loginErrorMessage' ).update( $('loginErrorMessage').innerHTML );
        }
      }

      function selectedLogin(returnURL){        
        document.atuoSingOnForm.action=returnURL;
        document.atuoSingOnForm.submit();
        //window.location = "/webapps/bbgs-autosignon-BBLEARN/B2login.jsp?userId=" + userId;
      }
    </script>
  
 
