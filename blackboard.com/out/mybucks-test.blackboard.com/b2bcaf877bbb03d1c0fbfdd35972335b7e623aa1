  <script type="text/javascript">
		function setLoginBoxOnload() {
			var useForm=(window.location.href).indexOf('internal')>0?true:false;
			setLoginBox(useForm);
		}
		function setLoginBox(useForm) {
			if (useForm) {
				// show normal login 
				document.getElementById('loginFormTitle').style.visibility='visible';
				document.getElementById('loginFormText').style.visibility='visible';
				document.getElementById('loginFormFields').style.visibility='visible';
				document.getElementById('loginFormTitle').style.display='';
				document.getElementById('loginFormText').style.display='';
				document.getElementById('loginFormFields').style.display='';
				// hide external provider logins
				document.getElementById('loginRedirectProviders').style.visibility='hidden';
				document.getElementById('loginRedirectProviders').style.display='none';
				document.getElementById('toggle').style.display='none';
				document.getElementById('toggle').style.visibility='hidden';
			}
			else {
				// hide normal login
				document.getElementById('loginFormTitle').style.visibility='hidden';
				document.getElementById('loginFormText').style.visibility='hidden';
				document.getElementById('loginFormFields').style.visibility='hidden';
				document.getElementById('loginFormTitle').style.display='none';
				document.getElementById('loginFormText').style.display='none';
				document.getElementById('loginFormFields').style.display='none';
				// HIDE external provider logins
				document.getElementById('loginRedirectProviders').style.visibility='hidden';
				document.getElementById('loginRedirectProviders').style.display='none';
				document.getElementById('toggle').style.display='';
				document.getElementById('toggle').style.visibility='visible';
			}
			
		}
	</script>

 
