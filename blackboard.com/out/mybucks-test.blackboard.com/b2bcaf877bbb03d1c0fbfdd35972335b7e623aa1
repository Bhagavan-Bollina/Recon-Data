<script type="text/javascript">
	function getParameterByName(name, url) {
		if (!url) url = window.location.href;		
		name = name.replace(/[\[\]]/g, '\$&');
		var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
		results = regex.exec(url);		
		if (!results) return '';
		if (!results[2]) return '';
		return decodeURIComponent(results[2].replace(/\+/g, ' '));
	}
	
	var redirectUrl = getParameterByName('redirectUrl');
	var link = ""
	if (!redirectUrl || 0 === redirectUrl.length) {
		redirectUrl = getParameterByName('new_loc');
	}
	
	link = document.getElementById("admin-link").getAttribute("href") + "&new_loc=" + encodeURIComponent(redirectUrl);
	document.getElementById("admin-link").setAttribute("href",link);
	
	if (redirectUrl) redirectUrl = window.location.origin + redirectUrl;
	
	link = document.getElementById("login-link").getAttribute("href") + encodeURIComponent(redirectUrl);
	document.getElementById("login-link").setAttribute("href",link);
</script>

<script>
function displayLoginHelp() {
  var x = document.getElementById("LoginHelpInfo");
  if (x.style.display === "none") {
    x.style.display = "block";
  } else {
    x.style.display = "none";
  }
}
</script>



