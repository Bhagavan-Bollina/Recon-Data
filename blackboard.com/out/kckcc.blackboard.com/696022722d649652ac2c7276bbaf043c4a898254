function debug_GetQueryString()
{

alert("debug_GetQueryString A");

var ls;
var namevalue;
var i;
var data;
var s;
var e1;
var getdata = [];
var length1000;


length1000 = Object.keys(getdata).length;
alert("getdata-length = " + length1000);

alert("location.search = " + location.search);
alert("location.search.length = " + location.search.length);

if (location.search.length > 1)
{
  ls = location.search.substring(1);
  namevalue = ls.split("&");
  for (i=0; i<namevalue.length; i++)
  {
    data = namevalue[i].split("=");
    getdata[data[0]] = data[1];
  }
}

s = "";
for (el in getdata)
{
  s += el + ": " + getdata[el] + "\n";
}

alert(s);

length1000 = Object.keys(getdata).length;
alert("getdata-length = " + length1000);

alert("debug_GetQueryString B");

return false;

}

//******************************************************************************************
//  END - Tools for Tracing and Debugging
