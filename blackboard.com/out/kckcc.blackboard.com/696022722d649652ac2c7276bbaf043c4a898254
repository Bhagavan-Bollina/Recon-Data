  <script type="text/javascript">

//********************************************************************************************************************
//*
//*  The following JavaScript will be executed when the page loads via the jQuery ready function
//*
//********************************************************************************************************************

var glob_default_login_url;

glob_default_login_url = "https://myclass.kckcc.edu/webapps/login?action=default_login";
//glob_default_login_url = "http://bbdev:8080/webapps/login/login.jsp";

$j(document).ready(function() {

//***** Begin jQuery ready block *****

var glob_queryString1000;

try
{
	trace_console_write("Begin Try Block");
		
	$j("body").removeClass();
	
	//RemoveStyleSheetNodes();
	//addCss('https://myclass.kckcc.edu/bbcswebdav/library/Custom_Branding_Login_Page/bb-css/bootstrap-min.css');
			
	glob_queryString1000 = location.search;

	
	if (glob_queryString1000.search(/classic/i) > 0)	
	{	
		location.href = glob_default_login_url;
	}	
	
	if (glob_queryString1000.search(/noannouncements/i) > 0)	
	{	
		//alert("parm noannouncements found");
	}	
	else
	{
		Branding_Get_Announcements_via_AJAX();		
	}	
	
	if (glob_queryString1000.search(/box/i) > 0)	
	{
		//alert("parm box found B");
		$j("#box2000").remove();		
	}	
	
	if (glob_queryString1000.search(/debug/i) > 0)	
	{
		//alert("debug found");
		$j(".debug_buttons").show();		
	}
	else
	{	
		//alert("default to nodebug");
		$j(".debug_buttons").hide();
	}	
	
	if (glob_queryString1000.search(/catch/i) > 0)	
	{	
		obj_error.error = 1;	//***** this will simulate a catch error ***** 
	}
			
	kckcc_remove_blog_cookie();	
	kckcc_page_complete();
	
	setTimeout(Detect_IE10_and_IE11,3000);
	
}
catch(e)
{
	alert("***** Catch Error *****");
	alert(e);
	
	trace_console_write("Catch Error");	
	trace_console_write(e);	
	
	location.href = glob_default_login_url;
	
}
finally
{
	trace_console_write("Begin Finally Block");
	//alert("finally1000");	
}

//***** End jQuery ready block *****

});

//********************************************************************************************************************
//  http://myclass.kckcc.edu/webapps/login/login.jsp
//  https://myclass.kckcc.edu/webapps/login?action=default_login
//********************************************************************************************************************

</script>

 
