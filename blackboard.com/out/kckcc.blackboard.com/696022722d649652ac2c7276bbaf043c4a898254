//*******************************************************************************************

function Branding_Get_Announcements_via_AJAX()
{
//alert("Branding_Get_Announcements AA");
trace_console_write("Branding_Get_Announcements A");

var loadUrl;

var obj1000;
var s1000;

loadUrl = glob_default_login_url;

$j("#branding_announcements_buffer").load(loadUrl  + " #loginAnnouncements",CallBack1000);

trace_console_write("Branding_Get_Announcements B");

return false;

}

//******************************************************************************************

function ListAnnouncementTitlesFromBuffer()
{

//alert("ListAnnouncements AA");

var obj1;
var obj2;
var obj3;
var obj4;
var i;
var s1;
var s2;
var s3;
var s4;
var date1000;

s1 = "<ul id='notify1000'>";

//***** Extract announcement items from buffer via class name *****

obj1 = document.getElementsByClassName("announcementDate");

//alert("Number of Announcements = " + obj1.length);

if (obj1.length == 0)
{
	obj4 = document.getElementById("AnnouncementList");
	obj4.innerHTML = "Welcome to Blackboard.";
}
else
{
	for (i=0; i< obj1.length ; i++)
	{
		//alert(obj1[i].innerHTML);
		s2 = obj1[i].innerHTML;		// Get announcement date
		obj2 = $j(obj1[i]);			// Add jQuery wrapper 
		obj3 = obj2.prev();			// Previous node is announcement title
		//alert(obj3.html());
		s3 = obj3.html();			// Get announcement title
		
		date1000 = s2.trim();
		date1000 = date1000.replace("(","");
		date1000 = date1000.replace(")","");
		
		//s4 = "<li>" + s3 + "&nbsp;&nbsp;&nbsp;&nbsp;" + s2 + "</li>";
		//s4 = "<li>" + s3  + "</li>";
		//s4 = "<li title='" + date1000 + "'>" + s3  + "</li>";
		s4 = "<li title='" + date1000 + "'><span>" + s3  + "</span></li>";
		
		
		s1 = s1 + s4;		
	}

	s1 = s1 + "</ul>";
	s1 = s1 + "<a id='ShowDetails' targetx='_blank' href='https://myclass.kckcc.edu/bbcswebdav/library/Custom_Branding_Login_Page/KCKCC_Blackboard_Announcements.htm'>Show Details</a>";
		
	obj4 = document.getElementById("AnnouncementList");
	obj4.innerHTML = s1;
}

//alert("ListAnnouncements B");
  
return false;

}

