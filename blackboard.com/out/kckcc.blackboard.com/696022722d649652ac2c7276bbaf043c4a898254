//*  https://stackoverflow.com/questions/16135814/check-for-ie-10
//*
//******************************************************************************************

function Detect_IE10_and_IE11()
{

//alert("Detect_IE10_and_IE11");

var s1;
var ver1;
var IE_Found
var UAString;

IE_Found = false
UAString = navigator.userAgent;

if (navigator.appVersion.indexOf("MSIE 10") !== -1)
{
	ver1 = "10";
	IE_Found = true;
}

if (UAString.indexOf("Trident") !== -1 && UAString.indexOf("rv:11") !== -1)
{
	ver1 = "11";
	IE_Found = true;
}

if (IE_Found)
{
	s1 = "Internet Explorer " + ver1 + " detected.\n\n";
	s1 = s1 + "Blackboard does not support this web browser.\n\n";
	s1 = s1 + "Please use Firefox or Google Chrome.";
	alert(s1);
}
else
{
	//alert("Web Browser is not IE");
}

return false;

}

