<script type="text/javascript">
  document.addEventListener('onload',function(){

    var currentLocation = window.location.href,
       substring = "course_id=_";
    if (currentLocation.indexOf(substring) != -1) {

        try {
            document.getElementById("Home").classList.remove("active");
            document.getElementById("Learn").classList.add("active");
            document.getElementById("Teach").classList.add("active");

        }
        catch(error) {
          console.log("Tab didn't match, cos I only should see one.");
        }
      }
  });
</script>
