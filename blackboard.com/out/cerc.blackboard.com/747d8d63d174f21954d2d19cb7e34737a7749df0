        "MMG": function() {
        	var _this = this;
        	
            if($("#sw-content-container10.ui-hp .ui-widget.app.multimedia-gallery").length) {
                var mmg = eval("multimediaGallery" + $("#sw-content-container10.ui-hp .ui-widget.app.multimedia-gallery:first").attr("data-pmi"));
                mmg.props.defaultGallery = false;

				var transitions = ["slide-in-fade", "slide-in-pieces", "fade-up-pieces", "fade-bounce-in"];
                
                $("#sw-content-container10.ui-hp .ui-widget.app.multimedia-gallery:first").csMultimediaGallery({
                    "efficientLoad" : true,
                    "imageWidth" : 1500,
                    "imageHeight" : 600,
                    "mobileDescriptionContainer": [768, 640, 480, 320], // [960, 768, 640, 480, 320]
                    "galleryOverlay" : false,
                    "linkedElement" : [], // ["image", "title", "overlay"]
                    "playPauseControl" : true,
                    "backNextControls" : true,
                    "bullets" : false,
                    "thumbnails" : false,
                    "thumbnailViewerNum": [4, 4, 3, 3, 2], // NUMERICAL - [960 view, 768 view, 640 view, 480 view, 320 view]
                    "autoRotate" : true,
                    "hoverPause" : true,
                    "transitionType" : "custom", // fade, slide, custom
                    "transitionSpeed" : 1.5,
                    "transitionDelay" : 4,
                    "fullScreenRotator" : false,
                    "fullScreenBreakpoints" : [960], // NUMERICAL - [960, 768, 640, 480, 320]
                    "onImageLoad" : function(props) {}, // props.element, props.recordIndex, props.mmgRecords
                    "allImagesLoaded" : function(props) {}, // props.element, props.mmgRecords
                    "onTransitionStart" : function(props) { // props.element, props.currentRecordIndex, props.currentGalleryIndex, props.nextRecordIndex, props.nextGalleryIndex, props.mmgRecords
                    	$(".mmg-container", props.element).attr("data-transition", transitions[Math.floor(Math.random() * transitions.length)]);
                        
                        adjustMobileMMG(props);
                    },
                    "onTransitionEnd" : function(props) {}, // props.element, props.currentRecordIndex, props.currentGalleryIndex, props.mmgRecords
                    "allLoaded" : function(props) {
                    	$("#hp-slideshow").removeClass("loading");
                    
                    	$(".mmg-slide", props.element).each(function() { // props.element, props.mmgRecords
                        	$(".mmg-description", this).wrap('<div class="mmg-slide-description-outer"></div>');
                            
                            var readMoreText = $(".mmg-description-link.read-more", this).text();
                            $(".mmg-description-link.read-more", this).html('<span>' + readMoreText + '</span>');
                            
                            var watchVideoText = $(".mmg-description-link.watch-video", this).text();
                            $(".mmg-description-link.watch-video", this).html('<span>' + watchVideoText + '</span>');
                        });
                        
                        $(".mmg-viewer", props.element).append('<div class="mmg-controls"></div>');
                        $(".mmg-controls", props.element).append($(".mmg-control.back", props.element), $(".mmg-control.play-pause", props.element), $(".mmg-control.next", props.element));
                        
                        adjustMobileMMG(props);
                    },
                    "onWindowResize": function(props) { // props.element, props.mmgRecords
                    	adjustMobileMMG(props);
                    }
                });
            }
            
            function adjustMobileMMG(props) {
                if(_this.GetBreakPoint() != "desktop") {
                    if(!$(".mmg-viewer .mmg-description-outer", props.element).length) {
                        $(".mmg-viewer", props.element).append($(".mmg-description-outer", props.element));
                    }
                    if(!$(".mmg-description-outer .mmg-description-link span", props.element).length) {
                        var linkText = "";
                        $(".mmg-description-outer .mmg-description-link", props.element).each(function() {
                            linkText = $(this).text();
                            $(this).html('<span>' + linkText + '</span>');
                        });
                    }
                }
            }
            
            if($("#sw-content-container7.ui-hp .ui-widget.app.multimedia-gallery").length) {
                var mmg = eval("multimediaGallery" + $("#sw-content-container7.ui-hp .ui-widget.app.multimedia-gallery").attr("data-pmi"));
                mmg.props.defaultGallery = false;
                
                $("#sw-content-container7.ui-hp .ui-widget.app.multimedia-gallery").csMultimediaGallery({
                    "imageWidth" : 750,
                    "imageHeight" : 465,
                    "mobileDescriptionContainer": [960, 768, 640, 480, 320], // [960, 768, 640, 480, 320]
                    "linkedElement" : ["image"], // ["image", "title", "overlay"]
                    "playPauseControl" : false,
                    "backNextControls" : false,
                    "bullets" : true,
                    "autoRotate" : false,
                    "hoverPause" : false,
                    "transitionType" : "fade", // fade, slide, custom
                    "transitionSpeed" : 1.5,
                    "transitionDelay" : 4,
                    "onImageLoad" : function(props) {}, // props.element, props.recordIndex, props.mmgRecords
                    "allImagesLoaded" : function(props) {}, // props.element, props.mmgRecords
                    "onTransitionStart" : function(props) { // props.element, props.currentRecordIndex, props.currentGalleryIndex, props.nextRecordIndex, props.nextGalleryIndex, props.mmgRecords
                    	$(".mmg-description-title", props.element).replaceWith($('<p class="mmg-description-title">' + props.mmgRecords[props.nextRecordIndex].title + '</p>'));
                    },
                    "onTransitionEnd" : function(props) { // props.element, props.currentRecordIndex, props.currentGalleryIndex, props.mmgRecords
                    	$(".mmg-bullet.active", props.element).focus();
                    },
                    "allLoaded" : function(props) {
                    	$(".mmg-description-outer", props.element).prepend('<h2 class="mmg-description-app-header">' + $(".ui-widget-header h1", props.element).text() + '</h2>');
                        $(".mmg-description-title", props.element).replaceWith($('<p class="mmg-description-title">' + props.mmgRecords[0].title + '</p>'));
                    },
                    "onWindowResize": function(props) {} // props.element, props.mmgRecords
                });
            }
        },
        
        "CheckSlideshow": function() {
        	// FOR SCOPE
            var _this = this;

            if($(".hp").length && this.GetBreakPoint() != "desktop") {
                if ($(window).scrollTop() <= $("#hp-slideshow ").offset().top + $("#hp-slideshow .ui-widget.app.multimedia-gallery").height()) {
                    if(this.SlideshowDescFixed) {
                        $("#hp-slideshow .ui-widget.app.multimedia-gallery").removeAttr("style");

                        this.SlideshowDescFixed = false;
                    }
                } else {
                    if(!this.SlideshowDescFixed) {
                        $("#hp-slideshow .ui-widget.app.multimedia-gallery").css({
                            "height": $("#hp-slideshow .ui-widget.app.multimedia-gallery").height(),
                            "overflow": "hidden"
                        });

                        this.SlideshowDescFixed = true;
                    }
                }
            } else {
            	if(this.SlideshowDescFixed) {
                    $("#hp-slideshow .ui-widget.app.multimedia-gallery").removeAttr("style");

                    this.SlideshowDescFixed = false;
                }
            }
        },
        
        "BioAppBuildView": function() {
            // BUILD THE TWO-COLUMN VIEW
            var i = 1;
            $(".ui-widget.app.sw-share-bio .ui-articles li").each(function() {
                $(".sw-share-bio-columns .sw-share-bio-column.column-" + i).append('<div class="sw-share-bio-item">' + $(this).clone().html() + '</div>');

                i++;
                if(i == 3) i = 1;
            });

            // MARK THE DESCRIPTIONS THAT ARE TRUNCATED 
            $(".sw-share-bio-columns .sw-share-bio-item").each(function() {
                // IF THERE THE DESCRIPTION IS TALLER THAN THE FIXED HEIGHT CONTAINER
                if($(".item-full-description > p", this).height() > $(".item-full-description", this).height()) {
                    $(".item-full-description > p", this).wrapInner('<span class="item-full-description-inner"></span>');
                    $(".item-full-description > p", this).append('<span class="item-full-description-truncated">' + $(".item-full-description > p .item-full-description-inner", this).html() + '</span>');
                    $(".item-full-description-truncated", this).ellipsis();
                }
            });

            // ITEM HOVER
            $(".sw-share-bio-item").hover(function() {
                $(".item-full-description", this).stop(true, true);

                // IF THERE THE DESCRIPTION IS TALLER THAN THE FIXED HEIGHT CONTAINER
                if($(".item-full-description > p", this).height() > $(".item-full-description", this).height()) {
                    $(".item-full-description", this).animate({
                        height: $(".item-full-description > p", this).height()
                    }, 300);
                }
            }, function() {
                $(".item-full-description", this).animate({
                    height: "148px"
                }, 300);
            });
        },
        
        "ParallaxScrolling": function() {
        	var scrollPosition = $(window).scrollTop();
            var halfScreen = $(window).scrollTop() + ($(window).height() * .50);
            var quarterScreen = $(window).scrollTop() + ($(window).height() * .25);
            var threeQuarterScreen = $(window).scrollTop() + ($(window).height() * .75);
            
            var hpContentOne = $(".hp-content.one").offset().top;
            
            if(hpContentOne <= quarterScreen) {
            	$(".hp-content.one").addClass("animate");
            }
        },

        "RsMenu": function() {
            $.csRsMenu({
                "breakPoint" : 768, // SYSTEM BREAK POINTS - 768, 640, 480, 320
                "slideDirection" : "right-to-left", // OPTIONS - left-to-right, right-to-left
                "menuButtonParent" : "#gb-mobile-menu",
                "menuBtnText" : "Menu",
                "colors": {
                    "pageOverlay": "#000000", // DEFAULT #000000
                    "menuBackground": "#FFFFFF", // DEFAULT #FFFFFF
                    "menuText": "#333333", // DEFAULT #333333
                    "menuTextAccent": "#333333", // DEFAULT #333333
                    "dividerLines": "#E6E6E6", // DEFAULT #E6E6E6
                    "buttonBackground": "#4776BE", // DEFAULT #E6E6E6
                    "buttonText": "#FFFFFF" // DEFAULT #333333
                },
                "showSchools" : false,
                "showTranslate" : true,
                "translateMenuText": "Translate",
                "translateVersion": 2,
                "showAccount": true,
                "accountMenuText": "Account",
                "usePageListNavigation": true,
                "siteID": "4",
                "allLoaded": function(){}
            });
        },
        
        "StickyMobileMenu": function() {
			if(!this.IsMyViewPage) {
                switch(this.GetBreakPoint()) {
                    case "desktop":
                        if(this.MobileMenuIsSticky) {
                            $("#gb-mobile-menu-outer").removeClass("sticky").removeAttr("style");

                            this.MobileMenuIsSticky = false;
                        }
                        
                        if($(".sp").length && $(".ui-widget.app.calendar").length) {
                            $(".wcm-controls").css("margin-top", "0px");
                        }
                        $("#sw-maincontent").removeAttr("style");
                    break;
                    case "768": case "640": case "480": case "320":
                        var menuOffSet = $("#gb-mobile-menu-outer").offset().top;
                        if ($(window).scrollTop() <= menuOffSet) {
                            if(this.MobileMenuIsSticky) {
                                $("#gb-mobile-menu-outer").removeClass("sticky").removeAttr("style");

                                this.MobileMenuIsSticky = false;
                            }
                        } else {
                            $("#gb-mobile-menu-outer").css("height", $("#gb-mobile-menu").outerHeight(true));

                            if(!this.MobileMenuIsSticky) {
                                $("#gb-mobile-menu-outer").addClass("sticky");

                                this.MobileMenuIsSticky = true;
                            }
                            
                            if($(".sp").length && $(".ui-widget.app.calendar").length) {
                            	if($(".wcm-controls").hasClass("wcm-stuck")) {
                                	$(".wcm-controls").css("margin-top", $("#gb-mobile-menu").outerHeight(true));
                                }
                            }
                        }
                        
                        $("#sw-maincontent").css({
                            "display": "block",
                            "position": "relative",
                            "top": "-" + $("#gb-mobile-menu").outerHeight(true) + "px"
                        });
                    break;
                }
            }
        },

        "AppAccordion": function() {
            $("#hp-content").csAppAccordion({
                "accordionBreakpoints" : [640, 480, 320]
            });

            $(".sp-column.one").csAppAccordion({
                "accordionBreakpoints" : [480, 320]
            });
        },

        "Search": function() {
            var _this = this;
            
            $(document).on("click keydown", function(e) {
            	setTimeout(function() {
                    if($("#gb-search").attr("aria-hidden") == "false" && !$("#gb-search :focus").length) {
                        $("#gb-search").attr("aria-hidden", "true");
                    }
                }, 400);
            });
            
            $(document).on("click keydown", "#gb-search-toggle", function(e) {
                if(_this.AllyClick(e)) {
                    e.preventDefault();

					if($("#gb-search").attr("aria-hidden") == "false") {
                    	$("#gb-search").attr("aria-hidden", "true");
                        $(".gb-site-utility.search").removeClass("open");
                    } else {
                    	$("#gb-search").attr("aria-hidden", "false");
                        $(".gb-site-utility.search").addClass("open");
                        
                        setTimeout(function() {
                        	$("#gb-search-input").focus();
                        }, 400);
                    }
                }
            });
            
            $("#gb-search-form").submit(function(e){
                e.preventDefault();
				
                if($.trim($("#gb-search-input").val()) != "" && $.trim($("#gb-search-input").val()) != "Search") {
                	window.location.href = "/site/Default.aspx?PageType=6&SiteID=4&SearchString=" + $("#gb-search-input").val();
                }
            });
            
            $("#gb-search-input").focus(function() {
                if($(this).val() == "Search") {
                    $(this).val("");
                }
            }).blur(function() {
                if($(this).val() == "") {
                    $(this).val("Search");
                }
            });
        },

        "AllyClick": function(event) {
            if(event.type == "click") {
                return true;
            } else if(event.type == "keydown" && (event.keyCode == this.KeyCodes.space || event.keyCode == this.KeyCodes.enter)) {
                return true;
            } else {
                return false;
            }
        },

        "GetBreakPoint": function() {
            return window.getComputedStyle(document.querySelector("body"), ":before").getPropertyValue("content").replace(/"|'/g, "");/*"*/
        }
    };
</script>

