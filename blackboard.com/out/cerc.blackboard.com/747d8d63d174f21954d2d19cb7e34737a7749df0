<script type="text/javascript">
    $(function() {
    	CreativeTemplate.Init();
    });
    
    var CreativeTemplate = {
    	// PROPERTIES
        "KeyCodes": { "tab": 9, "enter": 13, "esc": 27, "space": 32, "end": 35, "home": 36, "left": 37, "up": 38, "right": 39, "down": 40 },
        "IsMyViewPage": false,
        "ChannelBarIsSticky": false,
        "StickyMobileMenu": false,
        "SlideshowDescFixed": false,
        
        // METHODS
        "Init": function() {
            // FOR SCOPE
            var _this = this;
			
            this.SetTemplateProps();
            this.JsMediaQueries();
            this.MyStart();
            this.Header();
            this.ChannelBar();
            this.StickyChannelBar();
            this.MMG();
            this.CheckSlideshow();
            this.Body();
            this.Translate();
            this.BuildFeaturedHeadlines();
            this.TabbedRegion();
            this.BioAppBuildView();
            this.Shortcuts();
            this.GlobalIcons();
            this.SocialIcons();
            this.Footer();
            this.AppAccordion();
            this.Search();
            this.GlobalJs("NewWindowLinks");
            //this.ParallaxScrolling();
            
            $(window).load(function(){ _this.WindowLoad(); });
            $(window).resize(function(){ _this.WindowResize(); });
            $(window).scroll(function(){ _this.WindowScroll(); });
        },
        
        "SetTemplateProps": function() {
        	// MYVIEW PAGE CHECK
            if($(".myview").length) this.IsMyViewPage = true;
        },

        "WindowLoad": function() {
            this.GlobalJs("NewWindowLinks");
            
            // NAVIGATION APPS
            $("div.ui-widget.app.navigation ul").removeAttr("aria-hidden style");
        },

        "WindowResize": function() {
            this.JsMediaQueries();
            this.StickyChannelBar();
            this.StickyMobileMenu();
			this.Shortcuts();
            this.CheckSlideshow();
        },
        
        "WindowScroll": function() {
            this.StickyChannelBar();
            this.StickyMobileMenu();
            this.CheckSlideshow();
            //this.ParallaxScrolling();
        },
        
        "GlobalJs": function(action) {
        	if(action.indexOf("TcwMarkdown") > -1) {
				csGlobalJs.TcwMarkDown();
            }
            
            if(action.indexOf("NewWindowLinks") > -1) {
				csGlobalJs.OpenInNewWindowWarning();
            }
        },

        "JsMediaQueries": function() {
            switch(this.GetBreakPoint()) {
                case "desktop": 

                break;
                case "768":

                break;
                case "640":

                break;
                case "480":

                break;
                case "320":

                break;
            }
        },

        "MyStart": function() {
        	
        },
        
        "Translate": function() {
        	// ADD TRANSLATE
        	$(".cs-mystart-dropdown.translate .cs-dropdown").creativeTranslate({
                "type": 2
            });
            
            // BIND DROPDOWN EVENTS
            this.DropdownActions({
                "dropdownParent": ".cs-mystart-dropdown.translate",
                "dropdownSelector": ".cs-selector",
                "dropdown": ".cs-dropdown",
                "dropdownList": ".cs-dropdown-list"
            });
        },
        
        "DropdownActions": function(params) {
            /* ASSUMES THE FOLLOWING STRUCTURE
            <div class="cs-mystart-dropdown user-options">
                <div class="cs-selector" tabindex="0" aria-label="User Options" role="button" aria-expanded="false" aria-haspopup="true">User Options</div>
                <div class="cs-dropdown" aria-hidden="true" style="display:none;">
                    <ul class="cs-dropdown-list">
                        <li><a href="#">Link</a></li>
                    </ul>
                </div>
            </div>
            */
            
        	// FOR SCOPE
            var _this = this;

            var dropdownParent = params.dropdownParent;
            var dropdownSelector = params.dropdownSelector;
            var dropdown = params.dropdown;
            var dropdownList = params.dropdownList;
            
            $(dropdownParent + " " + dropdownList + " a").attr("tabindex", "-1");
            
            // MYSTART DROPDOWN SELECTOR CLICK EVENT
            $(dropdownParent).on("click", dropdownSelector, function(e) {
                e.preventDefault();
                
                if($(this).parent().hasClass("open")){
                	$("+ " + dropdownList + " a").attr("tabindex", "-1");
                    $(this).attr("aria-expanded", "false").parent().removeClass("open").find(dropdown).attr("aria-hidden", "true").slideUp(300, "swing"); 
                } else {
                	$(this).attr("aria-expanded", "true").parent().addClass("open").find(dropdown).attr("aria-hidden","false").slideDown(300, "swing");
                }
        	});
            
            // MYSTART DROPDOWN SELECTOR KEYDOWN EVENTS
            $(dropdownParent).on("keydown", dropdownSelector, function(e) {
                // CAPTURE KEY CODE
                switch(e.keyCode) {
                    // CONSUME LEFT AND UP ARROWS
                    case _this.KeyCodes.enter:
                    case _this.KeyCodes.space:
                        e.preventDefault();

                        // IF THE DROPDOWN IS OPEN, CLOSE IT
                        if($(dropdownParent).hasClass("open")){
                        	$("+ " + dropdown + " " + dropdownList + " a").attr("tabindex", "-1");
                            $(this).attr("aria-expanded", "false").parent().removeClass("open").find(dropdown).attr("aria-hidden", "true").slideUp(300, "swing"); 
                        } else {
                        	$(this).attr("aria-expanded", "true").parent().addClass("open").find(dropdown).attr("aria-hidden", "false").slideDown(300, "swing", function(){
                                $(dropdownList + " li:first-child a", this).attr("tabindex", "0").focus();
                            });
                        }
                    break;

                    // CONSUME TAB KEY
                    case _this.KeyCodes.tab:
                    	if($("+ " + dropdown + " " + dropdownList + " a").length) {
                        	$("+ " + dropdown + " " + dropdownList + " a").attr("tabindex", "-1");
                        	$(this).attr("aria-expanded", "false").parent().removeClass("open").find(dropdown).attr("aria-hidden", "true").slideUp(300, "swing");
                        }
                    break;

                    // CONSUME LEFT AND UP ARROWS
                    case _this.KeyCodes.down:
                    case _this.KeyCodes.right:
                        e.preventDefault();
						
                        $("+ " + dropdown + " " + dropdownList + " a").attr("tabindex", "-1");
                        $("+ " + dropdown + " " + dropdownList + " li:first-child > a", this).attr("tabindex", "0").focus();
                    break;
                }
            });
			
            // MYSTART DROPDOWN LINK KEYDOWN EVENTS
            $(dropdownParent).on("keydown", dropdownList + " li a", function(e) {
                // CAPTURE KEY CODE
                switch(e.keyCode) {
                    // CONSUME LEFT AND UP ARROWS
                    case _this.KeyCodes.left:
                    case _this.KeyCodes.up:
                        e.preventDefault();

                        // IS FIRST ITEM
                        if($(this).parent().is(":first-child")) {
                            // FOCUS DROPDOWN BUTTON
                            $(this).closest(dropdownList).find("a").attr("tabindex", "-1");
                            $(this).closest(dropdownParent).find(dropdownSelector).focus();
                        } else {
                            // FOCUS PREVIOUS ITEM
                            $(this).closest(dropdownList).find("a").attr("tabindex", "-1");
                            $(this).parent().prev("li").find("> a").attr("tabindex", "0").focus();
                        }
                    break;

                    // CONSUME RIGHT AND DOWN ARROWS
                    case _this.KeyCodes.right:
                    case _this.KeyCodes.down:
                        e.preventDefault();

                        // IS LAST ITEM
                        if($(this).parent().is(":last-child")) {
                            // FOCUS FIRST ITEM
                            $(this).closest(dropdownList).find("a").attr("tabindex", "-1");
                            $(this).closest(dropdownList).find("li:first-child > a").attr("tabindex", "0").focus();
                        } else {
                            // FOCUS NEXT ITEM
                            $(this).closest(dropdownList).find("a").attr("tabindex", "-1");
                            $(this).parent().next("li").find("> a").attr("tabindex", "0").focus();
                        }
                    break;
                    
                    // CONSUME TAB KEY
                    case _this.KeyCodes.tab:
                    	if(e.shiftKey) {
                        	e.preventDefault();
                            
                        	// FOCUS DROPDOWN BUTTON
                            $(this).closest(dropdownList).find("a").attr("tabindex", "-1");
                            $(this).closest(dropdownParent).find(dropdownSelector).focus();
                        }
                    break;

                    // CONSUME HOME KEY
                    case _this.KeyCodes.home:
                        e.preventDefault();

                        // FOCUS FIRST ITEM
                        $(this).closest(dropdownList).find("a").attr("tabindex", "-1");
                        $(this).closest(dropdownList).find("li:first-child > a").attr("tabindex", "0").focus();
                    break;

                    // CONSUME END KEY
                    case _this.KeyCodes.end:
                        e.preventDefault();

                        // FOCUS LAST ITEM
                        $(this).closest(dropdownList).find("a").attr("tabindex", "-1");
                        $(this).closest(dropdownList).find("li:last-child > a").attr("tabindex", "0").focus();
                    break;
                    
                    // CONSUME ESC KEY
                    case _this.KeyCodes.esc:
                        e.preventDefault();

                        // FOCUS DROPDOWN BUTTON AND CLOSE DROPDOWN
                        $(this).closest(dropdownParent).find(dropdownSelector).focus();
                        $(this).closest(dropdownList).find("a").attr("tabindex", "-1");
                        $(dropdownSelector).attr("aria-expanded", "false").parent().removeClass("open").find(dropdown).attr("aria-hidden", "true").slideUp(300, "swing");
                    break;
                }
            });
            
            $(dropdownParent).mouseleave(function(e) {
            	if(e.target.nodeName.toLowerCase() !== "select") {
					$(dropdownList + " a", this).attr("tabindex", "-1");
					$(dropdownSelector, this).attr("aria-expanded", "false").parent().removeClass("open").find(dropdown).attr("aria-hidden", "true").slideUp(300, "swing");
                }
            }).focusout(function() {
                var thisDropdown = this;
                
                setTimeout(function () {
                    if(!$(thisDropdown).find(":focus").length) {
                        $(dropdownSelector, thisDropdown).attr("aria-expanded", "false").parent().removeClass("open").find(dropdown).attr("aria-hidden", "true").slideUp(300, "swing");
                    }
                }, 500);
            });
        },

        "Header": function() {
            
        },

        "ChannelBar": function() {
        	var _this = this;
        
        	//ITEM DOES NOT EXIST IN STORAGE
            if(sessionStorage.getItem("cs-site-nav") === null) {

                // APPLY ACTIVE CHANNEL TEXT
                if($.trim($("#sw-channel-list-container #channel-navigation > li:first-child > a span").text()).toLowerCase() == "home") {
                    $("#sw-channel-list-container #channel-navigation > li:first-child > a span").prepend("CE Hub ");
                }

                // EXCLUDE IMPLEMENTATION - UNCOMMENTED BY BRENTON KELLY FOR HAIG DOUGLAS ON 2.13.17
                //$(".sw-mystart-dropdown.schoollist .sw-dropdown-list li a[href='/Domain/11']").parent().remove();

                // EXCLUDE Creative Services Meeting Request SITE - BRENTON KELLY 2.22.19
                //$(".sw-mystart-dropdown.schoollist .sw-dropdown-list li a[href='/Domain/15']").parent().remove();
                
                // EXCLUDE NEW CERC TEMPLATE SITE - BRENTON KELLY 10.11.19
        		$(".sw-mystart-dropdown.schoollist .sw-dropdown-list li a[href='/Domain/12']").parent().remove();

                // GRAB THE SITE LIST
                $(".sw-mystart-dropdown.schoollist .sw-dropdown-list > li").each(function() {
                	var siteName = $("a", this).text();
                    $("a", this).html('<span data-text="' + siteName + '">' + siteName + '</span>');
                });
                $("#gb-universal-navigation > ul").append('' +
                	'<li role="menuitem"><a href="/"><span data-text="CE Hub">CE Hub</span></a></li>' +
                    $(".sw-mystart-dropdown.schoollist .sw-dropdown-list").html() +
                '');

                // LOOP THROUGH THE SITES AND GET THE CHANNEL BARS
                $("#gb-universal-navigation > ul > li").each(function() {
                
                    // SET ITEM INSTANCE AND CHECK IF THIS ITEM IS ACTIVE
                    var thisItem = $(this);
                    $(this).addClass("gb-site").attr("role", "menuitem");
                    var thisItemIsActive = ("CE Hub" == $("> a", this).text());

                    // GO GET CHANNEL BARS FOR ALL SITES
                    $.ajax({
                        type: "GET",
                        url: $("a", thisItem).attr("href"),
                        cache: false,
                        async: false,
                        success: function(data){
                            // ADD THE CHANNEL AND SECTION DROPDOWNS
                            thisItem.append('<ul class="gb-channel-list" role="menu" aria-haspopup="true" aria-hidden="true">' + $(data).find("#channel-navigation").html() + '</ul>');

                            // ADJUST CLASSES AND ADD ACCESSIBILITY ATTRIBUTES
                            $(".hidden-channel, .hidden-sections, .dropdown-hidden", thisItem).remove();
                            $(".gb-channel-list > li:not(.hidden-channel)", thisItem).removeClass("sw-channel-item").addClass("gb-channel-item").attr("role", "menuitem").removeAttr("id");
                            $(".gb-channel-item > ul", thisItem).removeClass("sw-channel-dropdown").addClass("gb-section-list").attr("role", "menu").attr("aria-hidden", "true");
                            $(".gb-section-list > li", thisItem).addClass("gb-section-item").removeAttr("id").attr("role", "menuitem");
                            $(".gb-section-list", thisItem).parent().attr("aria-haspopup", "true");
                            $(".gb-channel-list > li", thisItem).each(function() {
                                if(!$(".gb-section-list > li", this).length){ 
                                    $(".gb-section-list", this).remove();
                                    $(this).removeAttr("aria-haspopup");
                                }
                            });
                            
                            $(".gb-channel-list", thisItem).children().wrapAll('<li class="gb-channel-list-inner"></li>');
                            
                            var columnNum = 3;
                            var endRange = 0;
                            var liNum = $(".gb-channel-list-inner > .gb-channel-item", thisItem).length;

                            // LOOP TO BUILD COLUMNS
                            for(var i = 0; i < columnNum; i++) {
                                // KEEP FROM ADDING EMPTY UL'S TO THE DOM
                                if(i < liNum) {
                                    // IF liNum / columnNum REMAINDER IS BETWEEN .0 AND .5 AND THIS IS THE FIRST LOOP ITERATION
                                    // WE'LL ADD 1 TO THE END RANGE SO THAT THE EXTRA LINK GOES INTO THE FIRST COLUMN
                                    if((liNum / columnNum) % 1 > 0.0 && (liNum / columnNum) % 1 < 0.5 && i == 0) {
                                        endRange = Math.round(liNum / columnNum) + 1;
                                    }

                                    // IF liNum / columnNum REMAINDER IS .5 AND THIS LOOP ITERATION IS NOT THE LAST ITERATION
                                    // WE'LL SUBTRACT 1 FROM THE END RANGE SO THAT WE DON'T HAVE AN EXTRA LINK WHERE IT DOESN'T BELONG
                                    else if((liNum / columnNum) % 1 == 0.5 && i >= (columnNum / i)) {
                                        endRange = Math.round(liNum / columnNum) - 1;
                                    }

                                    // NOTHING SPECIAL HERE - DIVIDE NORMALLY FOR THIS LOOP ITERATION
                                    else {
                                        endRange = Math.round(liNum / columnNum)
                                    }

                                    // WRAP THE LI'S
                                    $(".gb-channel-list-inner", thisItem).append('<div class="gb-custom-column gb-custom-column-' + (i + 1) + '"><ul></ul></div>');
                                    $(".gb-channel-list-inner > .gb-channel-item:nth-child(n+1):nth-child(-n+" + endRange + ")", thisItem).appendTo($(".gb-custom-column-" + (i + 1) + " > ul", thisItem));
                                }
                            }

                            if(thisItem.is(":last-child")) {
                                // REMOVE TAB INDEXES
                                $("#gb-site-channels a").removeAttr("tabindex");

                                // SET HOME CHANNEL NAMES
                                $(".gb-channel-list-inner").each(function() {
                                    if($.trim($(".gb-custom-column-1 > ul > li:first-child > a span", this).text()).toLowerCase() == "home") {
                                        $(".gb-custom-column-1 > ul > li:first-child > a span", this).prepend($(this).closest(".gb-site").find("> a").text() + " ");
                                    }
                                });

                                // STORE THE NAV STRUCTURE
                                var stuffToStore = $("#gb-site-channels").html();
                                sessionStorage.setItem("cs-site-nav", JSON.stringify(stuffToStore));

                                _this.ChannelBarFunctions();

                                // BUILD RESPONSIVE MENU
                                _this.RsMenu();
                                _this.StickyMobileMenu();
                            }
                        }
                    });
                });
            }
            
            // ITEM DOES EXIST IN STORAGE
            else {

                // APPLY THE STORED STRUCTURE
                $("#gb-site-channels").html(JSON.parse(sessionStorage.getItem("cs-site-nav")));
                
                // APPLY ACTIVE CHANNEL TEXT
                if($.trim($("#sw-channel-list-container #channel-navigation > li:first-child > a span").text()).toLowerCase() == "home") {
        			$("#sw-channel-list-container #channel-navigation > li:first-child > a span").prepend("CE Hub ");
                }
                
                _this.ChannelBarFunctions();
                
                // BUILD RESPONSIVE MENU
                _this.RsMenu();
                _this.StickyMobileMenu();
            }
            
            
        },
        
        "ChannelBarFunctions": function() {
        	var _this = this;
        
        	$("#gb-site-channels > li").each(function() {
                 if("CE Hub" == $("> a", this).text()) $(this).addClass("active");
            });
        
        	$(".gb-channel-list a").attr("tabindex", "-1");

            $("#channel-navigation .sw-channel-item").each(function() {
                if($(".sw-channel-dropdown", this).length && $(".sw-channel-dropdown > li", this).length) {
                    $(this).addClass("has-dropdown");
                } else {
                	$(".sw-channel-dropdown", this).remove();
                }
            });
        	
            // REGULAR CHANNEL BAR
            $(".sw-channel-item").unbind('hover');
            $(".sw-channel-item").hover(function(){
                var subList = $(this).children('ul');
                if ($.trim(subList.html()) !== "") {
                    subList.show();
                }
                $(this).addClass("hover");
            }, function(){
                $(".sw-channel-dropdown").hide();
                $(this).removeClass("hover");
            });
            
            // UNIVERSAL NAVIGATION BAR - SITE NAME HOVER
            $("#gb-site-channels").on({
                mouseenter: function () {
                    if(!$(this).hasClass("active")) {
                    	$(this).addClass("hover")
                        $("> a", this).removeClass("hide-triangle");
                        $(".gb-channel-list").hide().attr("aria-hidden", "true");
                        $("> .gb-channel-list", this).show().attr("aria-hidden", "false");
                    }
                    $(".gb-section-list").removeAttr("style").attr("aria-hidden", "true");
                },
                mouseleave: function () {
                	$(".gb-site").removeClass("hover");
                    $(".gb-channel-list").hide().attr("aria-hidden", "true");
                    $(".gb-section-list").removeAttr("style").attr("aria-hidden", "true");
                },
                focusin: function() {
                	$(this).addClass("hover");
                },
                focusout: function() {
                	$(".gb-site").removeClass("hover");
                }
            }, ".gb-site");
            
            // UNIVERSAL NAVIGATION BAR - SITE NAME FOCUS
            $("#gb-site-channels").on("focus", ".gb-site > a", function() {
            	if(!$(this).hasClass("hide")) {
                	$(this).removeClass("hide-triangle");
                    // OPEN CHANNEL LIST DROPDOWN IF THERE IS ONE
                    if(!$(this).parent().hasClass("active") && $(this).parent().find("> .gb-channel-list") && $(this).parent().find("> .gb-channel-list").css("display") == "none") {
                        $(".gb-channel-list").hide().attr("aria-hidden", "true");
                        $(this).parent().find("> .gb-channel-list").show().attr("aria-hidden", "false");
                    }
                }
            });
            
            // UNIVERSAL NAVIGATION BAR - SITE NAME KEYDOWN
            $("#gb-site-channels").on("keydown", ".gb-site > a", function(e) {
            	switch(e.keyCode) {
                
                	case _this.KeyCodes.space:
                    	e.preventDefault();
                        
                        $(this).removeClass("hide").parent().find("> .gb-channel-list").show().attr("aria-hidden", "false");
                        $(this).parent().find("> .gb-channel-list a").first().focus();
                    break;
                    
                    case _this.KeyCodes.up:
                    	e.preventDefault();
                        
                        $(this).parent().find("> .gb-channel-list a").last().focus();
                    break;
                    
                    case _this.KeyCodes.down:
                    	e.preventDefault();
                        
                        $(this).parent().find("> .gb-channel-list a").first().focus();
                    break;
                    
                    case _this.KeyCodes.left:
                    	e.preventDefault();
                    	
                        if($(this).parent().is(":first-child")) {
                        	$(".gb-site:last-child > a").focus();
                        } else {
                        	$(this).parent().prev(".gb-site").find("> a").focus();
                        }
                    break;
                    
                    case _this.KeyCodes.right:
                    	e.preventDefault();
                    	
                        if($(this).parent().is(":last-child")) {
                        	$(".gb-site:first-child > a").focus();
                        } else {
                        	$(this).parent().next(".gb-site").find("> a").focus();
                        }
                    break;
                }
            });
            
            // UNIVERSAL NAVIGATION BAR - CHANNEL LIST KEYDOWN
            $("#gb-site-channels").on("keydown", ".gb-channel-list", function(e) {
            	if(e.keyCode == _this.KeyCodes.esc) {
                	e.preventDefault();
                    
                    $(this).hide();
                    $(this).closest(".gb-site").find("> a").addClass("hide hide-triangle").focus();
                    
                    setTimeout(function() {
                    	$(".gb-site > a").removeClass("hide");
                    }, 100);
                }
            });
            
            // UNIVERSAL NAVIGATION BAR - CHANNEL/SECTION KEYDOWN
            $(".gb-channel-list").each(function() {
            	var link = $("a", this);
                var firstLinkIndex = $(link).index($(link).first());
                var firstLink = $(link).eq($(link).index($(link).first()));
                var lastLinkIndex = $(link).index($(link).last());
                var lastLink = $(link).eq($(link).index($(link).last()));

                $(link).keydown(function(e) {
                    var increment;
                    var thisIndex = link.index(this);
                    var newIndex = -1;

                    switch(e.keyCode) {
                        case _this.KeyCodes.up:
                        case _this.KeyCodes.left:
                            e.preventDefault();

                            if($(this).is(firstLink)) {
                                $(this).closest(".gb-site").find("> a").focus();
                            } else {
                                newIndex = thisIndex - 1;
                            }
                        break;

                        case _this.KeyCodes.down:
                        case _this.KeyCodes.right:
                            e.preventDefault();

                            if($(this).is(lastLink)) {
                                newIndex = firstLinkIndex;
                            } else {
                                newIndex = thisIndex + 1;
                            }
                        break;
                    }

                    if(newIndex != -1) {
                        link.eq(newIndex).focus();
                    }
                });
            });
            
            $(document).on("keydown", function(e) {
                setTimeout(function() {
                    if(!$("#gb-site-channels").find(":focus").length && $(".gb-channel-list[style*='block']").length) {
                        $(".gb-channel-list").hide().attr("aria-hidden", "true");
                    }
                }, 400);
            });
            
            // ADJUST CHANNEL DROPDOWN POSITIONS
            adjustChannelDropdowns();
            $(window).resize(function() { adjustChannelDropdowns(); });
            
            function adjustChannelDropdowns() {
            	var windowWidth = $(window).width();
                var channelListTopOffset = 0;
                var channelRightOffset = 0;
                var diff = 0;
                
                $("#channel-navigation .sw-channel-item").each(function() {
					channelRightOffset = $(this).offset().left + $(this).outerWidth(true);
                    diff = windowWidth - channelRightOffset;
                    
                    if(diff < 180) { // 180 IS HALF THE WIDTH OF THE DROPDOWN
                    	$(this).addClass("set-dropdown-right");
                    }
                });
                
                $("#gb-site-channels .gb-site").each(function() {
                	channelListTopOffset = $(this).position().top + $(this).outerHeight(true);
                    
                    $("> .gb-channel-list", this).css("top", channelListTopOffset);
                });
            }
        },
        
        "StickyChannelBar": function() {
			if(!this.IsMyViewPage) {
                switch(this.GetBreakPoint()) {
                    case "desktop":
                        var navOffSet = $("#gb-channel-list-outer").offset().top;
                        if ($(window).scrollTop() <= navOffSet) {
                            if(this.ChannelBarIsSticky) {
                                $("#gb-channel-list-outer").removeClass("sticky").removeAttr("style");

                                this.ChannelBarIsSticky = false;
                            }
                        } else if ($(window).scrollTop() > (navOffSet + $("#gb-channel-list").outerHeight(true))) {
                            $("#gb-channel-list-outer").css("height", $("#gb-channel-list").outerHeight(true));

                            if(!this.ChannelBarIsSticky) {
                                $("#gb-channel-list-outer").addClass("sticky");
                                $("#gb-channel-list").addClass("animated flipInX");
                                
                                setTimeout(function() {
                                	$("#gb-channel-list").removeClass("animated flipInX");
                                }, 1000);

                                this.ChannelBarIsSticky = true;
                            }
                            
                            if($(".sp").length && $(".ui-widget.app.calendar").length) {
                            	if($(".wcm-controls").hasClass("wcm-stuck")) {
                                	$(".wcm-controls").css("margin-top", $("#gb-channel-list").outerHeight(true));
                                }
                            }
                        }
                        
                        $("#sw-maincontent").css({
                            "display": "block",
                            "position": "relative",
                            "top": "-" + $("#gb-channel-list").outerHeight(true) + "px"
                        });
                    break;
                    case "768": case "640": case "480": case "320":
                        if(this.ChannelBarIsSticky) {
                            $("#gb-channel-list-outer").removeClass("sticky").removeAttr("style");

                            this.ChannelBarIsSticky = false;
                        }
                        
                        if($(".sp").length && $(".ui-widget.app.calendar").length) {
                            $(".wcm-controls").css("margin-top", "0px");
                        }
                        $("#sw-maincontent").removeAttr("style");
                    break;
                }
            }
        },

        "Body": function() {
        	// CHECK FOR NO APPS
            $(".hp-content-outer").each(function() {
            	if(!$(".ui-widget.app", this).length) {
                	$(this).addClass("no-apps");
                }
            });
        
            // AUTO FOCUS SIGN IN FIELD
            $("#swsignin-txt-username").focus();

            // APPLY RESPONSIVE DIMENSIONS TO CONTENT IMAGES
            $("div.ui-widget.app .ui-widget-detail img")
                .not($("div.ui-widget.app.multimedia-gallery .ui-widget-detail img"))
                .not($("div.ui-widget.app.gallery.json .ui-widget-detail img"))
                .not($("div.ui-widget.app.headlines .ui-widget-detail img"))
                .each(function() {
                    if ($(this).attr('width') !== undefined && $(this).attr('height') !== undefined) { // IMAGE HAS INLINE DIMENSIONS
                        $(this).css({"display": "inline-block", "width": "100%", "max-width": $(this).attr("width") + "px", "height": "auto", "max-height": $(this).attr("height") + "px"});
                    }
            });

            // ADJUST FIRST BREADCRUMB
            $("li.ui-breadcrumb-first > a > span").text("CE Hub Home");

            // USE CHANNEL NAME FOR PAGELIST NAV HEADER IF ONE IS NOT PRESENT
            if(!$("div.sp.column.one .ui-widget-header h1").length) {
                $("div.sp.column.one .ui-widget-header").append("<h1></h1>");
            }
            
            $(".ui-widget.app.headlines .ui-article-thumb").attr({"aria-hidden": "true", "tabindex": "-1"});
            $(".ui-widget.app.headlines .share-card-call-to-action").remove();
            
            // UTILITY OPTIONS
            $(".gb-site-utility").focusin(function() {
            	$(this).addClass("hover");
            }).focusout(function() {
            	$(this).removeClass("hover");
            });
            
            var userOptionsItems = "";
            
			
			// SIGNIN BUTTON
            if($(".sw-mystart-button.signin").length) {
            	$(".sw-mystart-button.signin a span").text("Admin");
				userOptionsItems += "<li>" + $(".sw-mystart-button.signin").html() + "</li>";
			}
			
			// REGISTER BUTTON
            if($(".sw-mystart-button.register").length) {
                userOptionsItems += "<li>" + $(".sw-mystart-button.register").html() + "</li>";
            }
			
            
            
			// ADD USER OPTIONS DROPDOWN TO THE DOM
            $(".cs-mystart-dropdown.user-options .cs-dropdown-list").html(userOptionsItems);
			
			// BIND DROPDOWN EVENTS
			this.DropdownActions({
            	"dropdownParent": ".cs-mystart-dropdown.user-options",
                "dropdownSelector": ".cs-selector",
                "dropdown": ".cs-dropdown",
                "dropdownList": ".cs-dropdown-list"
            });
            
            $("#sw-mystart-outer").remove();
            
           	$(".ui-widget.app.cs-call-to-action-buttons").closest("div[id^='pmi-']").prev("div[id^='pmi-']").find(".ui-widget.app").addClass("reduced-bottom-padding");
        },
        
        "BuildFeaturedHeadlines": function() {
        	var featuredHeadlinesNum = 2;
            
            $(".hp-content-outer[data-featured-headlines] .ui-widget.app.headlines:not(.share-card-headlines)").each(function() {
            	if($(".ui-articles > li", this).length) {
                    
                    // GET PAGE MODULE INSTANCE ID AND MODULE INSTANCE ID
                    var pmi = $(this).closest("div[id*='pmi-']").attr("id").replace("pmi-", "");
                    var mi = $(this).closest("div[id*='module-content-']").attr("id").replace("module-content-", "");
                    
                    // ADD DATA ATTRIBUTES TO APP
                    $(this).attr({
                    	"data-pmi": pmi,
                        "data-mi": mi
                    });

                    // BUILD THE MMG OBJECT
                    var headlinesGalleryStructure = '' +
                    	'<div id="mmg-container-' + pmi + '" class="mmg-container" data-gallery-type="custom" data-transition="fade" data-record-num="0" data-is-animating="false" data-play-state="playing" data-is-hovering="false" data-has-focus="false" data-is-touching="false" data-active-record-index="0" data-active-gallery-index="0">' +
                            '<div class="mmg-viewer">' +
                                '<div class="mmg-slides-outer">' +
                                    '<ul class="mmg-slides"></ul>' +
                                '</div>' +
                                '<div class="mmg-live-feedback mmg-ally-hidden" aria-live="polite"></div>' +
                            '</div>' +
                        '</div>' +
                        '<scr' + 'ipt type="text/javascript">' +
                            'var multimediaGallery' + pmi + ' = {' +
                                '"props": {' +
                                    '"defaultGallery": false,' +
                                    '"imageWidth" : 800,' +
                                    '"imageHeight" : 465,' +
                                    '"pageModuleInstanceId": "' + pmi + '",' +
                                    '"moduleInstanceId": "' + mi + '",' +
                                '},' +
                                '"records": []' +
                            '};' +
                        '</scr' + 'ipt>' +
                    '';

                    // ADD THE MMG OBJECT TO THE DOME
                    $(".ui-widget-detail", this).prepend(headlinesGalleryStructure);

                    // SET A REFERENCE TO THE MMG OBJECT
                    var headlinesGallery = eval("multimediaGallery" + pmi);

                    // ADD RECORDS TO THE MMG OBJECT
                    var thisArticle = "";
                    for(var i = 1; i <= featuredHeadlinesNum; i++) {
                    	if($(".ui-articles > li:nth-child(" + i + ")", this).length) {
                        	thisArticle = $(".ui-articles > li:nth-child(" + i + ")", this);
                        
                        	headlinesGallery.records.push({
                                "flexDataId": "0",
                                "hideTitle": "False",
                                "title": $(".ui-article-title > a", thisArticle).text(),
                                "hideCaption": "False",
                                "caption": $(".ui-article-description", thisArticle).text(),
                                "imageSrc": $(".ui-article-thumb .img img", thisArticle).attr("src"),
                                "imageWidth": $(".ui-article-thumb .img img", thisArticle).attr("width"),
                                "imageHeight": $(".ui-article-thumb .img img", thisArticle).attr("height"),
                                "imageAltText": $(".ui-article-thumb .img img", thisArticle).attr("alt"),
                                "isLinked": "True",
                                "linkUrl": $(".ui-article-title > a", thisArticle).attr("href"),
                                "linkText": "",
                                "openLinkInNewWindow": "False",
                                "videoLinkText": "",
                                "videoIsEmbedded": "False",
                                "videoIframe": ""
                            });
                        }
                    }
                    
                    // PUSH EMPTY TRAILING OBJECT TO MATCH MMG FUNCTIONALITY FOR THE PLUGIN
                    headlinesGallery.records.push({});
                    
                    // REMOVE THE HEADLINE ARTICLES FROM THE DOM NOW THAT THEY ARE READY FOR THE MMG PLUGIN
                    $(".ui-articles > li:nth-child(n+1):nth-child(-n+" + featuredHeadlinesNum + ")", this).remove();
                    
                    // WRAP THUMBNAILS WITH LINKS
                    $(".ui-articles > li", this).each(function() {
                    	$(".ui-article-thumb img", this).wrap('<a href="' + $(".ui-article-title a", this).attr("href") + '" target="' + $(".ui-article-title a", this).attr("target") + '" tabindex="-1" aria-hidden="true"></a>');
                    });;
                    
                    // ADD CSS FOR FIREFOX
                    $(this).append('' +
                    	'<style type="text/css">' +
                            '#pmi-' + pmi + ':before {' +
                                'content: "960";' +
                                'display: none;' +
                            '}' +
                            '@media (max-width: 1023px) {' +
                                '#pmi-' + pmi + ':before {' +
                                    'content: "768";' +
                                '}' +
                            '}' +
                            '@media (max-width: 767px) {' +
                                '#pmi-' + pmi + ':before {' +
                                    'content: "640";' +
                                '}' +
                            '}' +
                            '@media (max-width: 639px) {' +
                                '#pmi-' + pmi + ':before {' +
                                    'content: "480";' +
                                '}' +
                            '}' +
                            'media (max-width: 479px) {' +
                                '#pmi-' + pmi + ':before {' +
                                    'content: "320";' +
                                '}' +
                            '}' +
                        '</style>' +
                    '');
                    
                    // NOW RUN THE MMG PLUGIN
                    $(this).csMultimediaGallery({
                    	"efficientLoad" : false,
                        "imageWidth" : 800,
                        "imageHeight" : 465,
                        "mobileDescriptionContainer": [960, 768, 640, 480, 320], // [960, 768, 640, 480, 320]
                        "linkedElement" : ["image", "title"], // ["image", "title", "overlay"]
                        "playPauseControl" : false,
                        "backNextControls" : false,
                        "bullets" : true,
                        "autoRotate" : false,
                        "transitionType" : "fade", // fade, slide, custom
                        "transitionSpeed" : 1.5,
                        "onImageLoad" : function(props) { // props.element, props.recordIndex, props.mmgRecords
                        	$(".mmg-image", props.element).each(function() {
                            	$("> a", this).attr({"aria-hidden": "true", "tabindex": "-1"});
                            });
                        },
                        "allImagesLoaded" : function(props) {}, // props.element, props.mmgRecords
                        "onTransitionStart" : function(props) { // props.element, props.currentRecordIndex, props.currentGalleryIndex, props.nextRecordIndex, props.nextGalleryIndex, props.mmgRecords
                            $(".mmg-description-links", props.element).remove();
                        },
                        "onTransitionEnd" : function(props) {}, // props.element, props.currentRecordIndex, props.currentGalleryIndex, props.mmgRecords
                        "allLoaded" : function(props) {
                            $(".mmg-description-links", props.element).remove();
                        },
                        "onWindowResize": function(props) {} // props.element, props.mmgRecords
                    });
                }
                
                $(this).addClass("mmg-built");
            });
        },
        
        "TabbedRegion": function() {
        	var _this = this;
        
        	if($(".hp-content.three").length && false) {
            
            	var tabs = [
                	{
