  <script type="text/javascript">	
/* =====================================================
LOGIN PANEL 
===================================================== */

//move label when called on from statments below
function moveLabel() {
	if(jQuery(this).val().length || jQuery(this).is(':focus') )
	{
		jQuery(this).parent().find('label').css('top','-25px');

	}
	else
	{
		jQuery(this).parent().find('label').css('top','10px');
	}
}

//input field animation on focus
jQuery('#loginFormList input[type=text], #loginFormList input[type=password]').on('focus click blur', moveLabel);
//input field animation on change
jQuery('#loginFormList input[type=text], #loginFormList input[type=password]').change(moveLabel);
//input field animation on load
jQuery('#loginFormList input[type=text], #loginFormList input[type=password]').each(moveLabel);
	
//show alt login
jQuery('#alt-login-button').on('click', function() {
	if( !jQuery('#loginPanel').hasClass('open') ) {
		jQuery('#loginPanel').addClass('open');
		setTimeout(function() { jQuery('#user_id').focus() }, 25);
	} else {
		jQuery('#loginPanel').removeClass('open');
	}
});
	
//hide alt login
jQuery('#cancel-login-button').on('click', function() {
	jQuery('#loginPanel').removeClass('open');
});
/*jQuery('#loginWrapper').focusout(function() {
	setTimeout(function(){
		if( !jQuery('#loginWrapper').find(':focus').length > 0 && jQuery('#loginPanel').hasClass('open')) {
			//jQuery('#loginPanel').removeClass('open');
		}
	}, 10);
}).keyup(function(e) {
	if (e.keyCode === 27 && jQuery('#loginPanel').hasClass('open')) {
		jQuery('#loginPanel').removeClass('open');
	}
});*/
	
//get first announcement
var announcement = jQuery('#loginAnnouncements ul li').first();
var announcementTitle = announcement.children('strong').text();
if(announcementTitle.startsWith('Attention:') || announcementTitle.startsWith('Alert:')) {
	if(announcementTitle.startsWith('Alert:')) {
		jQuery('#announcementToast').addClass('alert');
	}
	jQuery('#announcementToast strong').text(announcementTitle);
	jQuery('#announcementToast span').text(announcement.children('.announcementDate').text().replace('(','').replace(')','') );
} else {
	jQuery('#announcementToast').hide();
}

//get special URLs from other places on page
jQuery('#guestLogin').attr('href', jQuery('#preview-as-guest a').attr('href') );
jQuery('#sso-login-button').attr('href', jQuery('#loginRedirectProviderList li a').attr('href') );

//open modal
jQuery('[data-modal]').on('keypress click', function() {
	jQuery('#' + jQuery(this).data("modal") ).show();
	jQuery('body').addClass('overflow-protection');
});

//close modal
jQuery('[data-modal-close]').on('keypress click', function() {
	jQuery('#' + jQuery(this).data("modal-close") ).hide();
	jQuery('body').removeClass('overflow-protection');
});

//original, unmodified JS
function loadLoginPage()
{
	if ( top != self )
	{
	  top.location.replace( self.location.href );
	}
	if(document.forms.login.user_id != undefined)
	{
	  document.forms.login.user_id.focus();
	}
	setTimeout("triggerScreenreaderAlert()", 500);
}

function triggerScreenreaderAlert()
{
	if ( document.getElementById( 'loginErrorMessage' ) )
	{
		$( 'loginErrorMessage' ).update( $('loginErrorMessage').innerHTML );
	}
}

</script>

 
