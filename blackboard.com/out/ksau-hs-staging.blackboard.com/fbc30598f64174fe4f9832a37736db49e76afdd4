  <script type="text/javascript">
                    //           Default function from blackboard
                    function loadLoginPage() {
                        if (top != self) {
                            top.location.replace(self.location.href);
                        }
                        if (document.forms.login.user_id != undefined) {
                            document.forms.login.user_id.focus();
                        }
                        setTimeout("triggerScreenreaderAlert()", 500);
                    }

                    function triggerScreenreaderAlert() {
                        if (document.getElementById('loginErrorMessage')) {
                            $('loginErrorMessage').update($('loginErrorMessage').innerHTML);
                        }
                    }

                    //            function for the innerpages icon arrows
                    function changeImage(icon) {
                        document.getElementById('awnIcon').src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_Icon-AWN.png";
                        document.getElementById('faqIcon').src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_Icon-FAQs.png";
                        document.getElementById('formIcon').src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_Icon-BlackboardForms.png";
                        document.getElementById('supportIcon').src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_Icon-SystemSupport.png";
                        document.getElementById('policyIcon').src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_Icon-Policies.png";
                        if (icon === 'awn') {
                            document.getElementById("awnIcon").src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_PanelArrowDownButton.png";
                        } else if (icon === 'faq') {
                            document.getElementById("faqIcon").src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_PanelArrowDownButton.png";
                        } else if (icon === 'form') {
                            document.getElementById("formIcon").src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_PanelArrowDownButton.png";
                        } else if (icon === 'support') {
                            document.getElementById("supportIcon").src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_PanelArrowDownButton.png";
                        } else if (icon === 'policy') {
                            document.getElementById("policyIcon").src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_PanelArrowDownButton.png";
                        }

                    }


                    $(document).ready(function () {
                        document.getElementById('loginErrorMessage').innerText = "The username or password you typed is incorrect.\nPlease try again";
                    });
                </script>
            
 
