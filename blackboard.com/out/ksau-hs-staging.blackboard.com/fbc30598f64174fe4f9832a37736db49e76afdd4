            <script>

                // function to detect which language is in use
                $(function () {
                    if ($('html').is(':lang(en-us)')) {
                        $('.arabic').hide();
                        $('.english').show();
                        $('.panel').css("margin-left", "6vh");
                        $("#user_id").attr("placeholder", "Username");
                        $("#password").attr("placeholder", "Password");
                    } else if ($('html').is(':lang(ar-sa)')) {
                        $('.english').hide();
                        $('.arabic').show();
                        $('.panel').css({"margin-right": "10vh", "padding-right": "10px"});
                        $('.question::after').css('float', 'left');
                        $("#user_id").attr("placeholder", "اسم المستخدم");
                        $("#password").attr("placeholder", "كلمة المرور");
                        $("#user_id").css("direction", "rtl");
                        $("password").css("direction", "rtl");
                        $("#loginBox .arabic").css("margin-right", "15vh");
                        $("#univLogo").css("margin-right", "15vh");
                        $("#loginBox #entry-login").css("margin-left", "16px");
                        $("#loginBox input").css("margin-left", "10px");
                        $("#footerimgs").css("margin-right", "5vh");
                        $("#welcomeTitleAR").css("margin-right", "20vh");
                        $("#myButton").css("margin-right", "20vh");
                        $("#supportAr").css("margin-top", "5px");
                    }
                });

                // this function will be called later, this is responsible for sliding the content
                function slideContent() {
                    $('.tab-panels .tabs li').on('click', function () {
                        var $panel = $(this).closest('.tab-panels');
                        $panel.find('.tabs li.active').removeClass('active');
                        $(this).addClass('active');
                        //figure out which panel to show
                        var panelToShow = $(this).attr('rel');
                        //hide current panel
                        $panel.find('.panel.active').slideUp(showNextPanel);
                        $("#announcements").css("visibility", "hidden");

                        //show next panel
                        function showNextPanel() {
                            $(this).removeClass('active');
                            $('#' + panelToShow).slideDown('slow', function () {
                                $(this).addClass('active');
                            });
                        }
                        ;
                        //slide down on click

                    });
                }
                ;

                //            $(function () {
                //                $('#navigation ul li.active').on('click', function () {
                //                    $('#navigation').animate({bottom: "3vh"}, 400, 'swing');
                //
                //                    //                    reset all icons to original
                //                    document.getElementById('awnIcon').src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_Icon-AWN.png";
                //                    document.getElementById('faqIcon').src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_Icon-FAQs.png";
