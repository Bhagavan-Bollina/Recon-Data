                $(function () {
                    $('.tabs li').on('click', function () {
                        var selectedPage = $(this).prop('id');
                        //document.getElementById('navigation').style.bottom = "90vh";
                        //$('#navigation').addClass('slideUp');
                        $('#navigation').animate({bottom: "+87vh"});

                    }, slideContent());
                });
                
                

                $(function () {
                    var selectedIcon = $('.icon').attr('src', "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_PanelArrowDownButton.png");

                    $('#loginpage , #homeIcon').on('click', function revertToOriginal() {
                        $('#navigation').animate({bottom: "3vh"}, 400, 'swing');

                        //                    reset all icons to original
                        document.getElementById('awnIcon').src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_Icon-AWN.png";
                        document.getElementById('faqIcon').src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_Icon-FAQs.png";
                        document.getElementById('formIcon').src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_Icon-BlackboardForms.png";
                        document.getElementById('supportIcon').src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_Icon-SystemSupport.png";
                        document.getElementById('policyIcon').src = "https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_Icon-Policies.png";

                        $('.panel #panel0').addClass('active');
                        $('.panel').css('display', 'none');
                        $("#announcements").css("visibility", "visible");

                    });

                });


                function faqControl() {
                    $("#panel2 .answer").hide();
                    $("#panel2 .question").on("click", function () {
                        var answer = $(this).next();
                        if ($(this).attr('data-icon') === "\u25bd") {
                            $(this).attr('data-icon', '\u25b3');
                            answer.slideDown();
                        } else {
                            $(this).attr('data-icon', '\u25bd');
                            answer.slideUp();
                        }
                    });
                }
                ;

                $(document).ready(function () {
                    faqControl();
                });

                $(document).ready(function () {
                    $("#user_id").attr("autocomplete", "off");
                    $("#password").attr("autocomplete", "off");
                });

                $(document).ready(function () {
                    var slidePanel = $(img).attr('src', 'https://ksau-hs-staging.blackboard.com/branding/themes/KSAU_theme/images/1_PanelArrowDownButton.png');

                });

