<script>


$('#recuperaBox .content-boton a').on('click',function(e){
	e.preventDefault();
	$('#loginBox').slideUp('fast',function(){
		if ($('#iframeAcceso').css('display') == "none")
		{
			$("#ingresoBox").hide();
		}else{
			$("#ingresoBox").show();

		}

		$('#iframeAcceso').slideToggle(400)

		});
})

$('#ingresoTradicional .content-boton a').on('click',function(e){
	e.preventDefault();
	$('#iframeAcceso').slideUp('fast',function(){ $('#loginBox').slideToggle(400) });
})


</script>


