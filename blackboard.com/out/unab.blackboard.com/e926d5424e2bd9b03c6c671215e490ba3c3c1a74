<script type="text/javascript">
    var current_lang = document.getElementsByTagName("html")[0].getAttribute("lang").substring(0,2).toLowerCase();
    var jQCustomTotal = null;

    var ctDictionary = {
            'warning': {
              'en': 'WARNING!',
              'es': '¡ALERTA!',
              'pt': 'Alerta!'
            },
            'messageOne': {
              'en': 'There are some empty grades that will be filled with 1, just because they should not affect the final grades calculated',
              'es': 'Las notas no calificadas serán llenadas con 1, para que las mismas no afecten los cálculos finales',
              'pt': 'Ele está fora do exame'
            },
            'messageTwo': {
              'en': 'We have found some activities without end date, so those will not be filled with 1 until they have a due date, remenber that empty grades can affect your final grades',
              'es': 'Se ha encontrado algunas actividades sin fecha de vencimiento, por lo tanto no serán marcadas con 1 hasta que se les coloque dicha fecha, recuerde que estas notas vacías pueden repercutir sus cálculos finales',
              'pt': 'Ele está fora do exame'
            },
            'alertOne': {
              'en': 'Your teacher will know about',
              'es': 'Estimado docente:<br>La nota ingresada debe ser entre 0 y 20 sin decimales!',
              'pt': 'Seu professor será informado sobre'
            },
            'buttonClose': {
              'en': 'CLOSE',
              'es': 'CERRAR',
              'pt': 'FECHAR'
            }
    };
    if( window.location.href.indexOf("/webapps/gradebook/do/instructor/enterGradeCenter?course_id") != -1){
        jQCustomTotal = jQuery.noConflict();
       // var CTElement = document.getElemntByClassName("actionMenuItem");
        ctAehFindGetParameter('course_id', function(aehCourseId){
            if( aehCourseId ){
                console.log("JSHACK is running!");
                var LstElement = document.getElementById("nav").getElementsByTagName("a");

                console.log("Inside Method");
                let i = 0;
                for (i=0; i<LstElement.length; i++)
                    if(LstElement && LstElement[i] && LstElement[i].innerText == "UNAB - Calcula Total") {
                        console.log(LstElement[i].innerText);
                        //LstElement[i].onclick(showModal());
                        console.log("Inside validation");
                        LstElement[i].addEventListener("click", ctShowModal);
                    }

                
            }
        });
        
    }
    function ctTranslate() {
       jQCustomTotal = jQuery.noConflict();
        jQCustomTotal("[data-translate]").each(function(){
            let key = jQCustomTotal(this).data('translate');
          //  console.log(key);
            jQCustomTotal(this).html(ctDictionary[key][current_lang] || ctDictionary[key]['en']  );
        });
    }
    if(window.location.href.indexOf("/webapps/gradebook/do/instructor/enterGradeCenter?course_id") != -1){
        console.log('Running JS HACK on grade center');
        ctGetValidCourses(1);
    }else{
      console.log('Aborting grade center hack, this is not a grade center location');
    }
    function ctShowModal(){
        console.log("Muestra Modal");
        ctLdbCreateModalAlert('messageOne', 10000);
    }
       function ctLdbCreateModalAlert(msg, t){
        let headTag = document.head;
        //var alertIcon = "/webapps/lnoh-gcvalid-BBLEARN/resources/imgs/warning.png";
        var alertIcon = "/bbcswebdav/institution/GC%20Validations/warning.png";

        if(document.getElementById('ctmodal_alert') != null){
             document.getElementById('ctmodal_alert').remove();
        }
       

        let link1 = document.createElement('link');
        link1.rel = 'stylesheet';
        //link1.href = '/webapps/lnoh-gcvalid-BBLEARN/resources/css/modal_alert.css';
        link1.href = '/bbcswebdav/institution/GC%20Validations/modal_alert.css';
        headTag.appendChild(link1);

        let ldbBodyPage = document.body;


        let divModal = document.createElement('div');
        divModal.setAttribute('class', 'modal');
        divModal.setAttribute('id', 'ctmodal_alert');
        divModal.style.display = 'none';
        ldbBodyPage.insertBefore(divModal, ldbBodyPage.childNodes[0]);

        let divContent = document.getElementById('ctmodal_alert');

        let divModalContent = document.createElement('div');
        divModalContent.setAttribute('id', 'modalAlertContet');
        divModalContent.setAttribute('class', 'modal-content');
        divContent.appendChild(divModalContent);
       // console.log("HOLA");
        let centerTag = document.createElement('center');
        let h2Tag = document.createElement('h2');
        let pTag = document.createElement('p');
        let bTag = document.createElement('b');
        bTag.setAttribute('data-translate', 'warning');
        pTag.appendChild(bTag);
        h2Tag.appendChild(pTag);
        centerTag.appendChild(h2Tag);

        let p2Tag = document.createElement('p');
        p2Tag.setAttribute('data-translate', 'messageOne');
        let p3Tag = document.createElement('p');
        p3Tag.setAttribute('id', 'countExist');
        //p3Tag.setAttribute('data-translate', 'exist');
        //p3Tag.setAttribute('data-translate', 'alertOne');
        p3Tag.innerHTML = ctDictionary[msg][current_lang];

        //centerTag.appendChild(p2Tag);
        centerTag.appendChild(p3Tag);

        let imgTag = document.createElement('img');
        imgTag.setAttribute('src', alertIcon);
        imgTag.setAttribute('alt', 'WARNING');
        imgTag.setAttribute('width', '200');
        imgTag.setAttribute('height', '200');

        let p4Tag = document.createElement('p');
        p4Tag.setAttribute('data-translate', 'alertOne');
        let p5Tag = document.createElement('p');
        p5Tag.setAttribute('data-translate', 'alertTwo');
        let p6Tag = document.createElement('p');
        p6Tag.setAttribute('data-translate', 'alertThree');

        let brTag = document.createElement('br');

        let buttonTag = document.createElement('button');
        buttonTag.setAttribute('id', 'ctCloseModal');
        buttonTag.setAttribute('onclick', 'ctCloseModal()');
        buttonTag.setAttribute('data-translate', 'buttonClose');

        centerTag.appendChild(imgTag);
        //centerTag.appendChild(p4Tag);
        //centerTag.appendChild(p5Tag);
        //centerTag.appendChild(p6Tag);
        centerTag.appendChild(brTag);
        centerTag.appendChild(buttonTag);

        divModalContent.appendChild(centerTag);

        ctTranslate();

        let modal = document.getElementById('ctmodal_alert');
        if(modal !== null || modal !== undefined){
            modal.style.display = 'block';
        }

        setTimeout(function(){ 
            console.log('Closing modal');
            ctCloseModal();
        }, t);
    }

    function ctCloseModal(){
        let modal = document.getElementById('ctmodal_alert');

        if(modal && modal.style ){
            modal.style.display = 'none'
        }
        ctDeleteModal();
    }

    function ctDeleteModal(){
       //jQCustomTotal = jQuery.noConflict();
        if(document.getElementById('ctmodal_alert') != null){
             document.getElementById('ctmodal_alert').remove();
        }
    }
    function ctAehFindGetParameter(parameterName, callback) {
        var result = null,
            tmp = [];
        var items = location.search.substr(1).split("&");
        for (var index = 0; index < items.length; index++) {
            tmp = items[index].split("=");
            if (tmp[0] === parameterName) result = decodeURIComponent(tmp[1]);
        }
        return callback(result);
    }
    function ctGetValidCourses(type){
        ctAehFindGetParameter('course_id', function(aehCourseId){
            if( aehCourseId ){
                var url = "https://" + window.location.hostname + "/webapps/lnoh-CustomTotal-BBLEARN/getNullGrades?course_id=" + aehCourseId;
                var xhr = new XMLHttpRequest();
                xhr.onreadystatechange = function() {
                    if (xhr.readyState == XMLHttpRequest.DONE) {
                        var rulesData = JSON.parse(xhr.responseText.replace(new RegExp("'", 'g'), "\""));
                        var courses = rulesData.result;
                        console.log("result of validation : " + courses);
                        if (courses){
                        	ctLdbCreateModalAlert('messageTwo', 10000);
                        }
                    }
                }

                xhr.open('GET', url, true);
                xhr.send(null);
            }else{
                console.log("IS not a course");
            }
        });
    }
</script>
<!-- END HACK : laur.customTotal -->

<!-- START HACK : laur.landingpage -->
<script type="text/javascript">
    var current_lang = document.getElementsByTagName("html")[0].getAttribute("lang").substring(0,2).toLowerCase();
    //var jQ = null;

    if( window.location.href.indexOf("/webapps/portal/execute/tabs/tabAction?tab_tab_group_id=_103_1") != -1){
        var jQ = jQuery.noConflict();
        document.getElementsByClassName("container")[0].style.padding = "0px";
        document.getElementsByClassName("container")[0].style.margin = "0px";
        document.getElementsByClassName("container")[0].style.border = "0px solid black";
        getUserRolLandingPage();
        
    }    

    function getUserRolLandingPage(){
        var url = "https://" + window.location.hostname + "/webapps/lnoh-B2Services-BBLEARN/get-check-role";
        var role;
        var xhr = new XMLHttpRequest();
        xhr.onreadystatechange = function() {
            if (xhr.readyState == XMLHttpRequest.DONE) {
                var rulesData = JSON.parse(xhr.responseText.replace(new RegExp("'", 'g'), "\""));
                //console.log(rulesData);
                console.log("function role: " + rulesData);
                role = rulesData;
                if(role){
                    document.getElementById("appTabList").style.display = "none";
                    document.getElementById("actionbar").style.display = "none";
                }else{
                    //console.log("It's working");
                    document.getElementById("appTabList").style.display = "true";
                    document.getElementById("actionbar").style.display = "true";
                };
                
            }
        }

        xhr.open('GET', url, true);
        xhr.send(null);
        
    }
</script>
<!-- END HACK : laur.landingpage -->

