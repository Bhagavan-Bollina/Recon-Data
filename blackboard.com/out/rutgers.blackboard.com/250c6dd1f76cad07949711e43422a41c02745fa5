      font-size: 1.3em;
   }


   /* NetID Login */
      .login-netid {
         border: 1px solid #eee;
         position: relative;
         background-color: rgba(255,255,255,.8);
         text-align: center;
      }

      /* a.button.expand.success.submit.button-1 */
      .button-login {
         display: inline-block;
         width: auto;
         margin: 5em auto;
         padding: .75em 3em;
         text-transform: none !important;
         font-size: 1.2em;
         color: #fff;
      }



   /* Community Login */
      .login-heading {
         margin: 2em 0 0;
         padding: 1em 0;
         border-top: 1px solid #ddd;
      }

      .js-heading {
         cursor: pointer;
      }

      .js-heading:hover:after {
         color: #555;
         content: "Show / Hide";
         float: right;
         font-size: 12px;
         line-height: 2; /* line up with "Community Login" text */
         text-transform: uppercase;
      }


   /* arrow icon before "Community Login" label */
      .icon {
         width: 16px;
         height: 15px;
         display: none; /* hide the icon initially so it can be shown if JavaScript is enabled */
      }

      .js-heading .icon {
         display: inline-block;
      }

      .icon-toggle {
         background: url(/branding/themes/ru_bb_2015/images/utilicons_arrows_dark.png) no-repeat 0 0;
      }

      .open {
         background: url(/branding/themes/ru_bb_2015/images/utilicons_arrows_dark.png) no-repeat 100% 0;
      }

   /* Initial state of community login is open */
      .login-community {
         max-height: 400px;
         overflow: hidden;
      }

   /* This class is toggled on .login-community using JavaScript to trigger the CSS3 transition effect | it collapses the login box */
      .notvisible {
         max-height: 0;
      }

   /* This class is added onclick of the "Community Login" heading so that toggling .notvisible triggers this transition */
      .transition {
         -webkit-transition: max-height .5s ease-in;
         -moz-transition: max-height .5s ease-in;
         -o-transition: max-height .5s ease-in;
         transition: max-height .5s ease-in;
      }

   /* needed to make the show / hide transition work */
      .login-community-inner {
         background-color: rgba(255, 255, 255, 0.8);
         border: 1px solid #eee;
         margin-bottom: 3em;
         padding: 2em;
      }

   /* Community login error message background */
      .bad {
         background-color: #d21034;
      }


   /* hide "Sign in using" heading and "Rutgers NetID Login" link */
      #loginRedirectProviders {
         display: none;
      }


   /* Security Tips Section */
      .section-security {
         border-top: 1px solid #ddd;
         padding-top: .5em;
      }


   /* Lock icon */
      .section-security img {
         float: left;
