<script>

// ACG : JavaScript to show / hide Community Login

   var DOMReady = function(a,b,c){b = document, c = 'addEventListener'; b[c] ? b[c]('DOMContentLoaded',a) : window.attachEvent('onload',a)}

   DOMReady(function(){

   // Add .notvisible which collapses login box onload with max-height: 0 used for transition
      document.getElementById('js-login-community').addClassName('notvisible');

   // Add js-heading class so Show / Hide can be displayed on hover only when JavaScript enabled
      document.getElementById('js-login-heading').addClassName('js-heading');

    });


// Trigger CSS transition effect by toggling .notvisible on login box
   function toggle_visibility(id) {

      var e = document.getElementById(id);
      var re = /(^| )notvisible( |$)/;

   // Add transition to login box for smooth open / close animation
      e.addClassName('transition');

      if(re.test(e.className)) {

         toggle_arrow('icon-toggle');
         e.removeClassName('notvisible');

      }
      else {

         toggle_arrow('icon-toggle');
         e.addClassName('notvisible');

      }

   }


// Flip arrow up / down
   function toggle_arrow(id) {

      var e = document.getElementById(id);
      var re = /(^| )open( |$)/;

      if(re.test(e.className)) {
         e.removeClassName('open');
      }
      else {
         e.addClassName('open');
      }

   }


</script>







