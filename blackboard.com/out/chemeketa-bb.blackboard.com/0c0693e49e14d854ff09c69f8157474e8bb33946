  <script type="text/javascript">
      function loadLoginPage()
      {
    	  if ( top != self )
    	  {
    		  top.location.replace( self.location.href );
    		}
    	  if(document.forms.login.user_id != undefined)
    		{
    		  document.forms.login.user_id.focus();
    		}
    	  setTimeout("triggerScreenreaderAlert()", 500);
      }
      
      function triggerScreenreaderAlert()
      {
    	if ( document.getElementById( 'loginErrorMessage' ) )
  	    {
    	  $( 'loginErrorMessage' ).update( $('loginErrorMessage').innerHTML );
  	    }
      }
    </script>
	
	<script type="text/javascript">
		
	  //This script grabs the dynamic SAML link from the 3rd party drop down
	
	  //and then sets the hyperlink to that value for a custom element with id "mySaml"
	
	  var rlinks = document.querySelectorAll("a[href*='/auth-saml/saml/login?apId']");
	
	  document.getElementById("mySaml").href = rlinks[0]
	
	</script>
	
	<script type='text/javascript'>
	
	  //This script creates a toggle function to show/hide the login form fields.
	
	  function loginToggle(where, pval, nval) {
	
		var table = document.getElementById(where.attributes.rel.value);
	
		where.value = (where.value == nval) ? pval : nval;
	
		table.style.display = (table.style.display == 'block') ? 'none' : 'block';
	
	  }
	
	</script>
	
	
  
