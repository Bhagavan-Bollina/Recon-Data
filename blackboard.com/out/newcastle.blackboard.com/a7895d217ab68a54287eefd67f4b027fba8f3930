  <script type="text/javascript">
    function showPrivacyPolicy() {
      cookieConsent = new CookieConsent('/webapps/privacy-disclosure/');
      cookieConsent.showPolicy( 'blackboard.platform.security.NonceUtil.nonce=login', true );
    }
  </script>

  <script type="text/javascript">
      function loadLoginPage()
      {
    	  if ( top != self )
    	  {
    		  top.location.replace( self.location.href );
    		}
    	  if(document.forms.login.user_id != undefined)
    		{
    		  document.forms.login.user_id.focus();
    		}
    	  setTimeout("triggerScreenreaderAlert()", 500);
      }

      function triggerScreenreaderAlert()
      {
    	if ( document.getElementById( 'loginErrorMessage' ) )
  	    {
    	  $( 'loginErrorMessage' ).update( $('loginErrorMessage').innerHTML );
  	    }
      }
	  
	  function toggleContent()
	  {
		  $('loginBox').classList.toggle("hideform");
		  $('loginBox').classList.toggle("showform");
	  }
      // change to local login reset page 
      document.observe("dom:loaded", function() {
          
		  // Editing password reset link - not used (13/11/19)
		  var el = $$('span.forgot > a');
          el[0].writeAttribute('onclick', '');
          el[0].writeAttribute('target', '_blank');

		  // Extracting okta anchor and removing the redirect providers drop down list (13/11/19)
          var redirect = $$('#loginRedirectProviderList > li > a');
          $('loginRedirectProviderList').remove();
          $('redirectProvidersDropdownButton').remove();
		  
		  // Places correct auth link extracted above on the custom login button (13/11/19)
		  var href = redirect[0].readAttribute('href');
		  $('okta-link').writeAttribute('href',href);

		  // Enables toggling of local login form and puts it to onclick of logo div (13/11/19)
		  var logo = $$('.logo');
		  logo[0].writeAttribute('onclick', 'toggleContent()');
		  
		  // Changes Username form field to 'UONline Support' (13/11/19)
		  var user = $$('#loginFormList > li:first-child > label');
		  user[0].innerHTML = "UONline Support";
		  
		  // Removes 'Installation Details' from the copyright information (13/11/19) - not required as of SAAS
		  // var copydetails = $$('.additionalInfo > p');
		  // copydetails[1].remove();		  
      });
    </script>
  
 
   <script type="text/javascript">
   FastInit.addOnLoad( function()
   {
            if ( window.DWREngine )
       {
        try {DWREngine.beginBatch();} catch(ignore) {}
       } 
                if (typeof(initEditors) == 'function') { initEditors(); }; 
                                                             if (window['org'] && window['org']['owasp']) { org.owasp.esapi.ESAPI.initialize(); }; 
                                                             quickLinks.createHelper();
                                                                  if ( window.DWREngine )
       {
         try {DWREngine.endBatch();} catch(ignore) {}
       }
                          BrowserSpecific.registerListeners();
                loadLoginPage();
               });
   </script>
       
