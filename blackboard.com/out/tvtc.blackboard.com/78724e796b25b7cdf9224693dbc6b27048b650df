	     <script type="text/javascript">
		 var annbtn = document.createElement("INPUT");
		  annbtn.setAttribute("type", "button");
		  annbtn.setAttribute("value", 'Announcments');
		  annbtn.setAttribute("class", "button expand");
		  annbtn.setAttribute("id", "login-announcment");
		  annbtn.setAttribute('onclick',"annpop('popDiv')");
		

			insertAfter(document.getElementById("entry-login"),annbtn);
			
            function annpop(div) {
				document.getElementById(div).style.display = 'block';
			}
			function annhide(div) {
                document.getElementById(div).style.display = 'none';
			}
			//To detect escape button
			document.onkeydown = function(evt) {
				evt = evt || window.event;
				if (evt.keyCode == 27) {
					annhide('popDiv');
				}
			};
			
			function insertAfter(referenceNode, newNode) {
				referenceNode.parentNode.insertBefore(newNode, referenceNode.nextSibling);
			}

        </script>
