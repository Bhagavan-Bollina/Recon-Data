 <script type="text/javascript">
 var stlccQLDiv = document.getElementById('stlccQuickLaunch');
 var stlccQLTitle = document.getElementById('stlccQuickLaunchTitle');
 var stlccQLArrow = document.getElementById('stlccQuickLaunchToggleArrow');
 var stlccQLMenu = document.getElementById('stlccQuickLaunchMenu');
 var stlccThisDocUrl = window.self.location;
 var stlccParentDocUrl = window.top.location;
 
 if (stlccParentDocUrl != stlccThisDocUrl){
   stlccQLDiv.style.display = 'none';
 }
 else {
   stlccQLDiv.addEventListener("mouseleave", function( event ) {
        stlccQLMenuHide();
   }, false);
 
   stlccQLTitle.addEventListener("focus", function( event ) {
        stlccQLMenuShow();
   }, false);
 
   stlccQLTitle.addEventListener("mouseenter", function( event ) {
        stlccQLMenuShow();
   }, false);
 }
 
 function stlccQLMenuShow() {
     stlccQLMenu.style.display = 'block';
   stlccQLArrow.setAttribute('src', '/images/ci/mybb/arrowUp-topnav.png');
   stlccQLArrow.setAttribute('alt', 'Collapse STLCC Quick Launch Menu');
   stlccQLMenu.focus();
 }
 
 function stlccQLMenuHide() {
   stlccQLMenu.style.display = 'none';
   stlccQLArrow.setAttribute('src', '/images/ci/mybb/arrowDown-topnav.png');
   stlccQLArrow.setAttribute('alt', 'Expand STLCC Quick Launch Menu');
 }
 
 function stlccQuickLaunchMenuToggle() {
   if (document.getElementById('stlccQuickLaunchMenu').style.display == 'block') {
   stlccQLMenuHide();
   }
   else {
   stlccQLMenuShow();
   }
 }
 </script>



