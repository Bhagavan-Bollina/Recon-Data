				div.vSpace {
						height: 15px;
				}

				/* emulate hr */
				div.hr,
				div.hrMobile {
					background-color: #bfbfbf;
					height: 1px;
				}

				div.hr {
						width: 80%;
						margin: 0 auto;
				}

				div.hrMobile {
						width: 100%;
				}

				#loginOptions,
				#loginOptionsMobile,
				#bbLogin {
						width: 100%;
						text-align: center;
						margin: 10px auto;
				}
		</style>

