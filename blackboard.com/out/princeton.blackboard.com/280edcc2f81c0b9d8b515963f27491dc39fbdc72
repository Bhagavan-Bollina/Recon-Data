  <script type="text/javascript">


            var isMobile = false;
            // Bb supplied javascript function
            function loadLoginPage() {
                if (top != self) {
                    top.location.replace(self.location.href);
                }
                if (document.forms.login.user_id != undefined) {
                    document.forms.login.user_id.focus();
                }
                setTimeout("triggerScreenreaderAlert()", 500);
            }

            function triggerScreenreaderAlert() {
                if (document.getElementById('loginErrorMessage')) {
                    $('loginErrorMessage').update(
                            $('loginErrorMessage').innerHTML);
                }
            }

            //
            /* function to process user action */
            function process(what) {
                if (what == 'netId') {
                    // mobile 'Princeton NetID' btn clicked
                    loginByNetId();
                } else if (what == 'other' || what == 'blackboardId') {
                    // mobile 'Other' btn or 'Blackboard ID' radio btn clicked
                    showBlackboardLogin();
                } else if (what == 'guest') {
                    // mobile 'Guest' btn clicked
                    loginAsGuest();
                } else if (what == 'continue') {
                    // 'Continue' btn clicked
                    selectLoginOption();
                } else if (what == 'bbCancel') {
                    // bb login 'Cancel' btn clicked
                    cancelBlackboardLogin();
                } else if (what == 'bbLogin') {
                    // bb login 'Login' btn clicked
                    submitBlackboardLogin();
                } else {
                    alert("Error: unrecognized process parameter: " + what);
                }
            }

            /* function to determine which login action to perform based on radio
               btn selection - applies to desktop only */
            function selectLoginOption() {
                var fm = $('loginOptionsForm');
                var loginOption = "";
                for (i = 0; i < fm.loginOption.length; i++) {
                    if (fm.loginOption[i].checked) {
                        loginOption = fm.loginOption[i].value;
                        break;
                    }
                }

                if (loginOption == "netId") {
                    loginByNetId();
                } else if (loginOption == "blackboardId") {
                    showBlackboardLogin();
                } else if (loginOption == "guest") {
                    loginAsGuest();
                } else {
                    // this scenario should not occur
                    alert("Please select a login option.");
                }

                return false;
            }

            /* function to login using netId */
            function loginByNetId() {
                var target = getHrefForCas();

                if (target.length > 0) {
                    window.location.href = target;
                } else {
                    alert("Error: loginByNetId not yet working");
                }
            }

            /* function to login as guest */
            function loginAsGuest() {
                var s = window.location.href;
                if (s.indexOf('?') > - 1 ){
                  //s.replace('?', '?action=guest_login&');
                  iStart = s.indexOf('action=');
                  if(iStart > - 1){
                    iEnd = s.indexOf('&', iStart);
                    if(iEnd < 0){
                      iEnd = s.length;
                    }
                    sActionValue = s.substring(iStart, iEnd);
                    sNew = s.replace(sActionValue, 'action=guest_login&');
                    window.location.href = sNew;
                  }else{
                    window.location.href  += "&action=guest_login";
                  }
                }else {
                  window.location.href = "/webapps/login?action=guest_login";
                }
            }

            /* function to submit the blackboard login form */
            function submitBlackboardLogin() {
                var fm = $('bbLoginForm');
                if (validate_form(fm)) {
                    fm.submit();
                }
            }

            /* function to display the blackboard login form */
            function showBlackboardLogin() {
                $('loginOptionsMobile').style.display = "none";
                ensureCancelButton();
                $('loginBox').style.display = "block";

                return false;
            }

            /* function to cancel (hide) the blackboard login form */
            function cancelBlackboardLogin() {
                $('loginOptionsMobile').style.display = "block";
                $("loginBox").style.display = "none";
								/* the line below places the cursor in the Username text field */
								document.getElementById("user_id").focus();
                return false;
            }

            /* function to create cookie */
            function createCookie(name, value, days) {
                var expires = "";
                if (days) {
                    var date = new Date();
                    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                    var expires = "; expires=" + date.toGMTString();
                }
                document.cookie = name + "=" + value + expires + "; path=/";
            }

            /* function to read cookie */
            function readCookie(name) {
                var nameEq = name + "=";
                var ca = document.cookie.split(';');
                for (var i = 0; i < ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0) == ' ') {
                        c = c.substring(1, c.length);
                    }
                    if (c.indexOf(nameEq) == 0) {
                        return c.substring(nameEq.length, c.length);
                    }
                }
                return null;
            }

            /* function to erase cookie */
            function clearCookie(name) {
                createCookie(name, "", -1);
            }

            /* function to check if user hit enter key */
            function checkEnter(evt) {
                // TODO check this for mobile
                var evt = (evt) ? evt : ((event) ? event : null);
                if (evt.keyCode == 13) {
                    if (isMobile) {
                    } else {
                        if ($('loginOptions')
                                && ($('loginOptions').style.display == "block" || $('loginOptions').style.display == "")) {
                            selectLoginOption();
                        } else if ($('loginBox')
                                && $('loginBox').style.display == "block") {
                            submitBlackboardLogin();
                        } else if (casLogout) {
                            window.location.href = "https://fed.princeton.edu/cas/logout";
                            return false;
                        } else {
                            return false;
                        }
                    }
                }
            }

            /* Build cancel button as document fragment */
            /* input class='' type="button" name="cancel" value="Cancel"*/
            function buildCancelButton() {
                var docFrag = document.createDocumentFragment();
                var newBtn = document.createElement("input");
                newBtn.setAttribute("class", "btn");
                newBtn.setAttribute("type", "button");
                newBtn.setAttribute("name", "cancel");
                newBtn.setAttribute("value", "Cancel");
                newBtn.setAttribute("id", "loginCancelBtn");
                newBtn.setAttribute("onclick", "process('bbCancel')");
                docFrag.appendChild(newBtn);
                return docFrag;
            }

            /* Take documentFragment of a cancel button and insert */
            function insertCancelButton(frag) {
                var ul = document.getElementById("loginFormList");
                var inputEls = ul.getElementsByTagName("input");
                var i, inputType, inputName, submitEl, parentEl;
                var insertedEl;

                for (i = 0; i < inputEls.length; i++) {
                    inputType = inputEls[i].getAttribute("type");
                    inputName = inputEls[i].getAttribute("name");
                    if (inputType == "submit" && inputName == "login") {
                        submitEl = inputEls[i];
                        parentEl = submitEl.parentNode;
                        insertedEl = parentEl.insertBefore(frag, submitEl);
                        break;
                    }
                }
                return insertedEl;
            }

            /* Ensure cancel button is created only if it doesn't exist */
            function ensureCancelButton() {
                var cancelBtn = document.getElementById("loginCancelBtn");
                var docFrag = null;
                if (!cancelBtn) {
                    docFrag = buildCancelButton();
                    cancelBtn = insertCancelButton(docFrag);
                }

            }

            /* Retrieve the value of the href used to redirect to CAS server */
            function getHrefForCas() {
                var ul = document.getElementById("loginRedirectProviderList");
                var aEls = ul.getElementsByTagName("a");
                var i, hrefVal;
                var anchorEl;
                /*var prefixToCheck = "/webapps/bb-auth-provider-cas-";*/
				var prefixToCheck = "/auth-saml/saml/login?";
                var prefixLen = prefixToCheck.length;
                for (i = 0; i < aEls.length; i++) {
                    anchorEl = aEls[i];
                    hrefVal = anchorEl.getAttribute("href");
                    if (hrefVal) {
                        if (prefixToCheck == hrefVal.substr(0, prefixLen)) {
                            break;
                        } else {
                            hrefVal = "";
                        }
                    } else {
                        hrefVal = "";
                    }
                }
                return hrefVal;
            }
            document.onkeypress = checkEnter;
        </script>
 
 
