  <script type="text/javascript">
      function loadLoginPage()
      {
    	  if ( top != self )
    	  {
    		  top.location.replace( self.location.href );
    		}
    	  if(document.forms.login.user_id != undefined)
    		{
    		  document.forms.login.user_id.focus();
    		}
    	  setTimeout("triggerScreenreaderAlert()", 500);
      }
      
      function triggerScreenreaderAlert()
      {
    	if ( document.getElementById( 'loginErrorMessage' ) )
  	    {
    	  $( 'loginErrorMessage' ).update( $('loginErrorMessage').innerHTML );
  	    }
      }
	  
//-------------------------------------------------------------------------------
// Below is PolyU owned Login button function.
// In order to seperate the Guest login and PolyU ADFS Login
//-------------------------------------------------------------------------------
var ssolink;	
//// Get all the links from the pages and assign to ssolink[array]
ssolink=document.links;

//// Run show Reset function at first
show_reset();
	
function show_sso(){
	loginbox_content
	document.getElementById( 'login_selection' ).style.display = 'none';
	document.getElementById( 'loginRedirectProviders' ).style.display = 'block';
	document.getElementById( 'loginFormFields' ).style.display = 'none';	
	document.getElementById( 'btn_cancel' ).style.display = 'block';
}
	
function show_guest(){
	document.getElementById( 'loginBox' ).style.display = 'block';	
	document.getElementById( 'login_selection' ).style.display = 'none';
	document.getElementById( 'loginRedirectProviders' ).style.display = 'none';
	document.getElementById( 'loginFormFields' ).style.display = 'block';	
	document.getElementById( 'btn_cancel' ).style.display = 'block';
}
	
function show_reset(){
document.getElementById( 'loginBox' ).style.display = 'none';	
document.getElementById( 'loginFormFields' ).style.display = 'none';
document.getElementById( 'loginRedirectProviders' ).style.display = 'none';
document.getElementById( 'btn_cancel' ).style.display = 'none';
document.getElementById( 'login_selection' ).style.display = 'block';
} 

function getssolink(){
// =====================================
// This function is to Get original SSO Link from Shibbo And assign to polyu net id button
//
// @ 2015-08-11 The SSO links is = 0; ssolink.item(0)
// @ 2019-08-21 Please check the available links from checkssolink() Because it is mobile enabled loginpage
// For UAT site is 7
//window.location.href=ssolink.item(7)
// For PROD site is 6, Further check with the lowest part of SSO link check.
	window.location.href=ssolink.item(6)
}

function checkssolink(){
/* For Debug only */
document.getElementById("demo").innerHTML = '';
for(i=0; i<ssolink.length;i++){
	document.getElementById("demo").innerHTML = document.getElementById("demo").innerHTML + '(' + i + ')' + ssolink.item(i) + '<BR><BR>';
}
/* For Debug only */
}
	  
