	<div class="wpb_raw_code wpb_content_element wpb_raw_html" >
		<div class="wpb_wrapper">
			<div class="vc_row wpb_row vc_row-fluid bb-hero vc_custom_1545427559388 vc_row-has-fill">
 <div class="wpb_column vc_column_container vc_col-sm-12">
  <div class="vc_column-inner"><div class="wpb_wrapper">		
   <section id="hero" class="bb-hero bb-hero-bbtlcasia wavy">
    <div class="row">
     <div class="small-12 columns">
      <div class="bb-hero-inside">
       <div class="bb-hero-text">
         <img class="bb-hero-logo" src="https://tlclatam.bbworld.com/wp-content/uploads/sites/10/2020/02/hero-logo-bbtlclatam.png" alt="">
       </div>
