<script type="text/javascript">
  FastInit.addOnLoad(function () {
    jQuery('.bbModal').ModalDialogs();
    var $window = jQuery(window);
    var helpButtonParent = jQuery('.help-button').parent();

    jQuery('.help-button').on('click', function (e) {
      e.preventDefault();
      var drp = jQuery(e.target.parentElement);

      function hideHelpMenu() {
        if (!(drp.data['activating'] === true)) {
          drp.removeClass('active');
          jQuery('body div:first').off('click.hideHelp');
          jQuery('.help-button').attr("aria-expanded", false);
          var drpContent = drp.children('.dropdown-content');
          drpContent.css('top', 'auto');
          drpContent.css('left', '0');
        }
      }

      if (drp.hasClass('active')) {
        hideHelpMenu();
      } else {
        jQuery('.help-button').attr("aria-expanded", true)
        drp.data['activating'] = true;
        drp.addClass('active');
        
        /* start of check and fix off-screen position */
        var clientHeight = $window.height();
        var clientWidth = $window.width();
        var drpContent = drp.children('.dropdown-content');
        var width = drpContent.width();
        var height = drpContent.height();
        var loc = drpContent.offset();
        var bounds = drpContent[0].getBoundingClientRect();
        if (bounds.bottom >= clientHeight || bounds.right >= clientWidth) {
          // reposition menu to fit on screen
          loc.left -= width - jQuery('.help-button').width();
          loc.top -= drpContent.outerHeight() + jQuery('.help-button').height();
          drpContent.offset(loc);
        } else {
          drpContent.css('top', 'auto');
          drpContent.css('left', '0');
        }
        
        /* end of check and fix off-screen position */
        jQuery('body').on('click.hideHelp', function hideHelpDropdown() {
          hideHelpMenu();
        })
      }
      window.setTimeout(function () {
        delete drp.data['activating'];
      }, 0);
    });
  });

  /* Modal */
  (function ($) {

    /* private global vars */
    var debug = false;
    var $window = $(window);
    var defaults = {
      modalWidth: '50%',
      modalHeight: '50%',
      modalOverlayColor: '#262626ba',
      modalColor: '#ffffff',
      modalButtonBarColor: '#f0f0f0',
      modalButtonClass: 'button',
      modalDividerColor: '#8d8d8d'
    };

    var modalTemplateSelector = '#modalTemplate', modalTemplate,
      modalOverlaySelector = '#modalOverlay', modalOverlay,
      modalDialogSelector = '#modalDialog', modalDialog,
      modalSelector = '#modal', modal,
      modalContentSelector = "#modalContent", modalContent,
      modalCloseSelector = '.modal-close', modalClose,
      modalButtonBarSelector = '#modalButtons', modalButtonBar;
    var prevFocusedElm;

    /* public extension */
    $.fn.ModalDialogs = function (options) {
      var settings = $.extend(defaults, options);
      this.on('click.showModal', function (e) {
        e.preventDefault();
        showModal($(this), settings);
      });
      return this;
    };

    /* private functions */
    function consoleError() {
      if (!console || !console.error) {
        return;
      }
      console.error(arguments);
    }

    function getModal() {
      if (!modalTemplate) {
        modalTemplate = $($(modalTemplateSelector).html());
        if (!modalTemplate || (modalTemplate.length && modalTemplate.length !== 1)) {
          consoleError("Modal: template missing - id[" + modalTemplateSelector + "]");
          return false;
        }
      }
      if (!modal || (modal.length && modal.length !== 1)) {
        $('body').append(modalTemplate);
        modal = $(modalSelector);
        if (!modal || (modal.length && modal.length !== 1)) {
          consoleError("Modal: unable to locate new modal - id[" + modalSelector + "]");
          return false;
        }
        modalOverlay = $(modalOverlaySelector);
        modalDialog = $(modalDialogSelector);
        modalContent = $(modalContentSelector);
        modalClose = $(modalCloseSelector);
        modalButtonBar = $(modalButtonBarSelector);
      }
      return modal;
    }

    function showModal(link, defaultSettings) {
      var clientWidth = $window.width() / 2;
      var clientHeight = $window.height() / 2;
      var settings = {};
      $.extend(settings, defaultSettings, link[0].dataset);
      window.debug = window.debug || {};
      window.debug.settings = settings;
      var iframeVersion = settings['modalLink'] && settings['modalLink'] !== '';
      var title = settings['modalTitle'] && settings['modalTitle'] !== '' ?
        '<h2 class=".m-header-text .large">' + settings['modalTitle'] + '</h2>'
        : '';
      if (!getModal()) {
        return false;
      }
      modalContent.html('');
      switch (iframeVersion) {
        case true:
          var iframe = '<iframe src="' + settings['modalLink'] + ' width="100%" height="100%"/>';
          modalContent.html(title + iframe);
          break;
        default:
          modalContent.html(title + settings['modalContent']);
          break;
      }

      modalOverlay.on('click.outside', function (e) {
        e.preventDefault();
        e.stopPropagation();
      });
      modalClose.on('click.closeModal', function (e) {
        e.preventDefault();
        destroyModal();
      });

      modalOverlay.css('background-color', settings['modalOverlayColor']);
      modalDialog.css('background-color', settings['modalColor']);

      modalButtonBar.css('border-top', '2px solid ' + settings['modalDividerColor']);
      modalButtonBar.css('background-color', settings['modalButtonBarColor']);
      modalButtonBar.children('.modal-button').addClass(settings['modalButtonClass']);
      prevFocusedElm = $(document.activeElement);
      modal.show(function () {
        var modalWidth = modalDialog[0].offsetWidth / 2;
        var mLeft = clientWidth - modalWidth;
        modalDialog.css('left', mLeft);
        modalDialog.focus();

      });
    }

    function destroyModal() {
      if (modal) {
        modal.remove();
        modal = false;
        prevFocusedElm.focus();
      }
    }

  }(jQuery));

</script>
<script type="template" id="modalTemplate">
  <div class="modal" id="modal" style="" aria-live="assertive">
    <div class="modal-overlay" id="modalOverlay">
    </div>
    <div class="modal-dialog" id="modalDialog" tabindex="0" role="main">
      <div class="modal-x modal-close" id="modalX">
        <button class="modal-button modal-close" id="modalCloseX">
          X
        </button>
      </div>
      <div class="modal-content" id="modalContent">
      </div>
      <div class="modal-buttons" id="modalButtons">
        <button class="modal-button modal-close button" id="modalCloseButton">Close</button>
      </div>
    </div>
  </div>
</script>
